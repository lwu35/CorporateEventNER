    margin-right: 50px;
}

#irwWrapper .summit-foundation-work .rows .image-holder .inner {
    width: 525px;
    max-width: 100%;
    background-position: center center;
    background-repeat: no-repeat;
    height: 400px;
    background-size: cover;
}

#irwWrapper .summit-foundation .content {
