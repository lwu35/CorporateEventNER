                    flCategory = "arcbe0";
                    break;
                case "/abf-freight":
                    flType = "legac0";
                    flCategory = "abffr0";
                    break;
            }

            if (typeof flType !== 'undefined') {
                var axel = Math.random() + "";
                var a = (axel * 10000000000000 + "").replace(".", "");
