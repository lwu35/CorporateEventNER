<script type="text/javascript">
$("#clndr").eventCal({
    calendar: {
        clickEvents: {
            click: function(target) {
                $("#latestEvents .selectedEvent").hide();
                $("#latestEvents .close").click(function(){
                    $("#latestEvents .selectedEvent").hide();
                });

                if (target.events.length) {
                    var html = [];
                    var events = [];
                    $.each(target.events, function(index, val) {
                        var eventItem = val.items;
                        var webcast =  (eventItem.WebCastLink !=='undefined') ? '<a href="' + eventItem.WebCastLink + '" class="WebCastLink"><i class="fa fa-microphone"></i> Listen to webcast</a>' :'' ;
                        var url = eventItem.LinkToUrl.length ? eventItem.LinkToUrl : eventItem.LinkToDetailPage;
                        var target = eventItem.LinkToUrl.length ? '_blank' : '_self';
                        html = [
                            '<div class="EventItem">',
								'<div class="body">',
                                    '<div class="EventTitle"><a href="'+eventItem.LinkToDetailPage+'" class="EventLink" target="'+target+'">' + eventItem.Title + '</a></div>',
                                '</div>',
                                '<div class="date">' + moment(eventItem.StartDate).format('MM/DD/YYYY') + '</div>',
                                
                            '</div>'
                        ].join('');
                        events.push(html);
                        $("#latestEvents .selectedEvent").find('.eventContent').html(events.join(''));
                        $("#latestEvents .selectedEvent").fadeIn(500);
                    });
                }
            }
        }
    }
});
</script>
