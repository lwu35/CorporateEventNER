		<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//clr.investorroom.com/js/wd_widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","prn_wjs");</script>

			<script>
			function getWidgetData() {
			  //Get Current Price
			  var widgetOpen = jQuery('#wd_hidden .wd_widget .wd_price span').text();

		      if(widgetOpen == '') {
				    setTimeout(getWidgetData, 1000);
			    } else {
				//Get Change Value
				var widgetChange = jQuery('#wd_hidden .wd_widget .wd_price .wd_value').text();
				//Get Value Up or Down - Assign Conditional
				var widgetChangeClass = jQuery('#wd_hidden .wd_widget .wd_price .wd_value').hasClass('wd_quote-value-up') ? 'ccbnPos' : 'ccbnNeg';
				//Add Arrow Image Based on Up or Down Conditional
				//var widgetChangeImg = (widgetChangeClass == 'wd_quote-value-up') ? 'upGreen':'downRed';
				//Add Class Based on Up or Down Conditional
				var widgetChangeText = (widgetChangeClass == 'wd_quote-value-up') ? 'up':'down';
				//Get Change Percent
				var widgetChangePercent = jQuery('#wd_hidden .wd_widget .wd_data2 .wd_value').text();
				//Get Volume
				var widgetVolume = jQuery('#wd_hidden .wd_widget .wd_data5 .wd_value').text();
				//Get Market Cap
				var widgetMarketCap = jQuery('#wd_hidden .wd_widget .wd_data6 .wd_value').text();
		        widgetChangePercent = widgetChangePercent.split('(');
		        widgetChangePercent = widgetChangePercent[1].substr(0,widgetChangePercent[1].length - 1);
		        jQuery("#wd_widget_open").append(widgetOpen);
		        jQuery("#wd_widget_change").append('<span class="'+ widgetChangeClass +'">'+widgetChange+'</span>');
		        jQuery("#wd_widget_percent_change").append('<span class="'+ widgetChangeClass +'">'+ widgetChangePercent +'</span>');
		        jQuery("#wd_widget_volume").append(widgetVolume);
		        jQuery("#wd_widget_market_cap").append(widgetMarketCap);

			    // update CLR top widget
			    jQuery('#ccbnStockQuoteBlock .view-content').append('<span class="ccbnTxtWhite">' + widgetOpen +'</span>&nbsp&nbsp;<span class="'+ widgetChangeClass +'2">'+ widgetChange +'</span>');
			  }
			}
			jQuery(document).ready(getWidgetData());
			</script>

			<script type="text/javascript">
				jQuery(document).ready(function(){

					//News Releases FB - Structure
					jQuery('.wd_newsfeed_releases-teaser .item').each(function(){
						jQuery(this).wrap('<div class="col-12 col-md-6"></div>');
					});

					//Events FB - Structure
					jQuery('#wd_featurebox-teaser_7 .wd_imagetable').wrap('<div class="col-12 col-xl-4 wd_left"></div>');
					//$('#wd_featurebox-teaser_7 p.more').insertAfter('.item');
					jQuery('#wd_featurebox-teaser_7 .wd_events-teaser').find('.item').unwrap();
					jQuery('#wd_featurebox-teaser_7 .wd_events-teaser').wrap('<div class="col-12 col-xl-8 wd_right"></div>');

					//Webcasts FB - Structure
					var webcastTitle = jQuery('#wd_featurebox-standard_6').find('.wd_featurebox_title').text();
					jQuery('#wd_featurebox-standard_6 .wd_imagetable').wrap('<div class="col-12 col-xl-4 wd_left"></div>');
					jQuery('#wd_featurebox-standard_6 .wd_left').after('<div class="wd_fb_title">'+webcastTitle+'</div>');
					jQuery('#wd_featurebox-standard_6 .wd_featurebox').find('.wd_fb_title, p').wrapAll('<div class="col-12 col-xl-8 wd_right"></div>');

					//Presentations FB - Structure
					var presentationsTitle = jQuery('#wd_featurebox-standard_9').find('.wd_featurebox_title').text();
					jQuery('#wd_featurebox-standard_9 .wd_imagetable').wrap('<div class="col-12 col-md-6 wd_left"></div>');
					jQuery('#wd_featurebox-standard_9 .wd_left').after('<div class="wd_fb_title">'+presentationsTitle+'</div>');
					jQuery('#wd_featurebox-standard_9 .wd_featurebox').find('.wd_fb_title, p, p.more').wrapAll('<div class="col-12 col-md-6 wd_right"></div>');

					//Wrap Committee Compostion Table for Mobile Scrolling
					
					//Wrap Selects 
					jQuery('#wd_printable_content select').wrap('<div class="wd_select-wrap"></div>');

					//Move Labels into Input Fields
					jQuery('form .wd_form_field').each(function() {
						var labelText = jQuery(this).find('.wd_form_field_label label').text();
						jQuery(this).find('.wd_form_field_input input').attr('placeholder', labelText);
						jQuery(this).find('.wd_form_field_input textarea').attr('placeholder', labelText);
					});

					//Events Page
											jQuery('button.wd_events_input_go').text('Go');
					

				    // Attachment Image Convert to FontAwesome
				    jQuery('.wd_attachment_image, .wd_attachment_icon').each(function(){
				        var fileTypeURLArray = jQuery(this).find('img').attr('src').split('/');
				        var fileTypeFile = fileTypeURLArray[fileTypeURLArray.length - 1];
				        switch(fileTypeFile) {
				            case 'avi.gif':var fileTypeFontAwesome = 'fa-file-video-o';break;
				            case 'm4v.gif':var fileTypeFontAwesome = 'fa-file-video-o';break;
				            case 'mov.gif':var fileTypeFontAwesome = 'fa-file-video-o';break;
				            case 'mp4.gif':var fileTypeFontAwesome = 'fa-file-video-o';break;
				            case 'mpg.gif':var fileTypeFontAwesome = 'fa-file-video-o';break;
				            case 'doc.gif':var fileTypeFontAwesome = 'fa-file-word-o';break;
				            case 'docm.gif':var fileTypeFontAwesome = 'fa-file-word-o';break;
				            case 'docx.gif':var fileTypeFontAwesome = 'fa-file-word-o';break;
				            case 'dotm.gif':var fileTypeFontAwesome = 'fa-file-word-o';break;
				            case 'dotx.gif':var fileTypeFontAwesome = 'fa-file-word-o';break;
				            case 'gif.gif':var fileTypeFontAwesome = 'fa-file-image-o';break;
				            case 'jpg.gif':var fileTypeFontAwesome = 'fa-file-image-o';break;
				            case 'mp3.gif':var fileTypeFontAwesome = 'fa-file-audio-o';break;
				            case 'wav.gif':var fileTypeFontAwesome = 'fa-file-audio-o';break;
				            case 'pdf.gif':var fileTypeFontAwesome = 'fa-file-pdf-o';break;
				            case 'potm.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'potx.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'ppam.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'ppsm.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'ppsx.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'ppt.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'pptm.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'pptx.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'sldm.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'sldx.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'thmx.gif':var fileTypeFontAwesome = 'fa-file-powerpoint-o';break;
				            case 'txt.gif':var fileTypeFontAwesome = 'fa-file-text-o';break;
				            case 'xlam.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xls.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xlsb.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xlsm.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xlsx.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xltm.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'xltx.gif':var fileTypeFontAwesome = 'fa-file-excel-o';break;
				            case 'zip.gif':var fileTypeFontAwesome = 'fa-file-archive-o';break;
				            default:var fileTypeFontAwesome = 'fa-file-o';break;
				        }
				        jQuery(this).find('a').append('<i class="fa '+fileTypeFontAwesome+'"></i>');
				    });

				    //Hiding Mobile Subnav Elements and Showing Subnav on Click

				    jQuery('.wd_investor-mobile-sub').hide();
