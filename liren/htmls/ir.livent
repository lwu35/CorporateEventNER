            if (type == 'opt-out' && !didConsent) {
                deleteAllCookies();
            }
        },
        // when a button is clicked on the banner
        onStatusChange: function(status, chosenBefore) {
            var type = this.options.type;
            var didConsent = this.hasConsented();
 
            if (type == 'opt-out' && !didConsent) {
                deleteAllCookies();
            }
            if (type == 'opt-out' && didConsent) {
                // set cookie sample
                // setCookie('cookieconsent-status','accept','.csgi.com'); // add client domain
                // setCookie('cookieconsent-timestamp',setTimestamp(),'.csgi.com'); // add client domain
                setCookie('cookieconsent-status','accept','.q4web.com');
                setCookie('cookieconsent-timestamp',setTimestamp(),'.q4web.com');
            }
        },
        // extra function refer to plugin documentation
        onRevokeChoice: function() {
            var type = this.options.type;
 
            if (type == 'opt-out') {
                // enable cookies
            }
        },
    });
 
    // used to close the banner without setting a cookie
    $('.module-cookie_close-button').on('click', function() {
        $(this).parents('.cc-window').hide();
    });
 
    // used in a privacy policy / cookie policy page to change current compliance selection
    $('.show-cookie').on('click', function(e) {
        e.preventDefault();
 
        //delete cookie sample
        deleteCookie('cookieconsent_status');
 
        cookiePopup();
    })

    $($('.cc-deny').clone()).appendTo('#deny-btn');
    $('.cc-highlight .cc-deny').hide();
}
 
// use to set a different cookie for custom client needs
function setCookie(cname, cvalue, cdomain) {
    document.cookie = cname + "=" + cvalue + ";" + " domain=" + cdomain + ";path=/";
};
 
// deletes a cookie
function deleteCookie(name) {
    document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}
 
// deletes all cookies
function deleteAllCookies() {
    var cookies = document.cookie.split(";");
 
    for (var i = 0; i < cookies.length; i++) {
        if (!cookies[i].includes("cookieconsent_status")) {
            var cookie = cookies[i];
            var eqPos = cookie.indexOf("=");
            var name = eqPos > -1 ? cookie.substr(0, eqPos) : cookie;
            document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT; domain=' + wildCardDomain() + ';';
            document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT; domain=' + wildCardDomain(true) + ';';
        }
    }
}
function setTimestamp() {
    var d = new Date();
    var month = d.getMonth() < 10 ? '0' + d.getMonth() : d.getMonth();
    var day = d.getDay() < 10 ? '0' + d.getDay() : d.getDay();
    var year = d.getFullYear().toString().substr(-2);
 
    var timestamp = month+day+year;
 
    return timestamp;
}
 
/* cookie management */
function wildCardDomain(subDomain) {
    if (subDomain === undefined) {
        subDomain = false;
    }
    var domaintouseforcookie = document.domain;
 
    // We cannot explicitly specify domain if it doesn't have any dots in it
    if (domaintouseforcookie.indexOf('.') < 0 || domaintouseforcookie.indexOf("127.") === 0) {
        domaintouseforcookie = "";
    } else {
        if (subDomain) {
            domaintouseforcookie = "." + domaintouseforcookie;
        }
        else {
            // We are going to be setting cookie for parent domain
            var splitDomains = domaintouseforcookie.split(".");
            splitDomains[0] = "";
 
            domaintouseforcookie = splitDomains.join(".");
        }
    }
    return domaintouseforcookie;
}
