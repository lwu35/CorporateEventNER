			function getWidgetData() {
			  //Get Current Price
			  var widgetOpen = jQuery('#wd_hidden .wd_widget .wd_price span').text();

		      if(widgetOpen == '') {
				    setTimeout(getWidgetData, 1000);
			    } else {
				//Get Change Value
				var widgetChange = jQuery('#wd_hidden .wd_widget .wd_price .wd_value').text();
				//Get Value Up or Down - Assign Conditional
				var widgetChangeClass = jQuery('#wd_hidden .wd_widget .wd_price .wd_value').hasClass('wd_quote-value-up') ? 'ccbnPos' : 'ccbnNeg';
				//Add Arrow Image Based on Up or Down Conditional
				//var widgetChangeImg = (widgetChangeClass == 'wd_quote-value-up') ? 'upGreen':'downRed';
				//Add Class Based on Up or Down Conditional
				var widgetChangeText = (widgetChangeClass == 'wd_quote-value-up') ? 'up':'down';
				//Get Change Percent
				var widgetChangePercent = jQuery('#wd_hidden .wd_widget .wd_data2 .wd_value').text();
				//Get Volume
				var widgetVolume = jQuery('#wd_hidden .wd_widget .wd_data5 .wd_value').text();
				//Get Market Cap
				var widgetMarketCap = jQuery('#wd_hidden .wd_widget .wd_data6 .wd_value').text();
		        widgetChangePercent = widgetChangePercent.split('(');
		        widgetChangePercent = widgetChangePercent[1].substr(0,widgetChangePercent[1].length - 1);
		        jQuery("#wd_widget_open").append(widgetOpen);
		        jQuery("#wd_widget_change").append('<span class="'+ widgetChangeClass +'">'+widgetChange+'</span>');
		        jQuery("#wd_widget_percent_change").append('<span class="'+ widgetChangeClass +'">'+ widgetChangePercent +'</span>');
		        jQuery("#wd_widget_volume").append(widgetVolume);
		        jQuery("#wd_widget_market_cap").append(widgetMarketCap);

