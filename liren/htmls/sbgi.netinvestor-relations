$('.scroll').on('click', function(e) {
	e.preventDefault();
	var target = $(this).attr('href');
	var offset = 0;
	if ($(this).data('offset') != undefined) offset = $(this).data('offset');
	$.scrollTo(target, 100, { offset: -offset });
});
