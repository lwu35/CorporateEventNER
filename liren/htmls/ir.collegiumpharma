                <article id="post-23" class="post-23 page type-page status-publish hentry">
                  <div class="entry-content">
                    <h1 class="main-title">
                                                Event Calendar
                                          </h1>
                    <div id="ndq-content" class="ndq-content ndq-5826">
                        <div class="region region-content">
    <div data-drupal-messages-fallback class="hidden"></div>
<div id="block-nir-pid1944-content" class="block--system-main-block block--system-main-block--5826 block--content--system-main-block block--content--system-main-block--5826 block--8ecc8378-c736-4325-9b9b-af65d5cf1b3a block--8ecc8378-c736-4325-9b9b-af65d5cf1b3a--5826 block block-system block-system-main-block">
  
    
      <article class="node node--type-nir_landing_page node--view-mode-full clearfix">
  
      <h1>
              Event Calendar
          </h1>
    

  <div>
    
<div  class="panel-display boxton clearfix">

  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 radix-layouts-content panel-panel">
        <div  class="panel-panel-inner">
          <div class="block-region-contentmain"><div class="block--upcoming-events block--nir-events__widget block--nir-events__widget--5826 block--contentmain--nir-events__widget block--contentmain--nir-events__widget--5826 block--f62275d6-ac61-4398-ad65-25fad8b131eb block--f62275d6-ac61-4398-ad65-25fad8b131eb--5826 block block-nir-events block-nir-events__widget">
  
    
      <div class="nir-widget">
      <div class="nir-widget--label">
      Upcoming Events
    </div>
