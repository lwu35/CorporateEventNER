<script src="https://widgets.q4app.com/widgets/q4.stockQuote.1.0.15.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $('.footer-stock').stockQuote({
        changeCls: ['text-danger', 'text-success'],
        studioApi: 'hannonarmstrong2020index.studio.q4api.com',
        stock: [Q4Settings.indices[0].xvalue, Q4Settings.indices[0].tickerLabel],
        stockTpl: (
            /* beautify preserve:start */
            '{{#.}}' +
                '<span class="footer-stock-ticker" title="HASI">Price: ${{tradePrice}}<br> ' +
                        'Change: <span class="stock_quote {{uod}}">{{change}}</span> (<span class="stock_quote {{uod}}">{{percChange}}%</span>) ' +
                        '<br> ' +
                        'Volume: {{volume}} ' +
                    '</span> ' +
            '{{/.}}'
            /* beautify preserve:end */
        ),beforeRender: function(e, data){
        $.each(data.items, function(i, item) {
            if (Math.sign(item.percChange) === -1) {
                item.percChange = '('+ Math.abs(item.percChange) + ')';
            }
            if (Math.sign(item.change) === -1) {
                item.change = '('+ Math.abs(item.change) + ')';
            }
        });

        }
    });
</script>

<script type="text/javascript">
    $('#media_image-2').financials({
