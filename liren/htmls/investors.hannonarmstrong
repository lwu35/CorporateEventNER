</script>
<script>
            $(window).scroll(function () {
                var scroll = $(window).scrollTop();
                if (scroll >= 800) {
                    $(".et_pb_scroll_top").addClass("et-visible");
                    $(".et_pb_scroll_top").removeClass("et-hidden");
                } else {
                    $(".et_pb_scroll_top").removeClass("et-visible");
                    $(".et_pb_scroll_top").addClass("et-hidden");
                }
            });

            $('.et_pb_scroll_top').on('click', function(e) {
                e.preventDefault();
                window.scroll({
                    top: 0,
                    behavior: 'smooth'
                });
            });
</script>

<script>
           equalheight = function (element) {
                $(element).height("auto");
                var currentTallest = 0
                $(element).each(function () {
                    if ($(this).outerHeight() > currentTallest) {
                        currentTallest = $(this).outerHeight();
                    }
                });
                $(element).outerHeight(currentTallest);
            }
            $(document).ready(function () {
                $(".presentation-console").appendTo(".presentation-dispaly").show();
                $("#TotalReturnCalculator").trigger("click");
                $("#footer_info_text").appendTo(".footer_info_text .container");
            });
            $(window).on("load", function () {
                if ($(window).width() > 768) {
                    equalheight(".sameheight");
                }
                else {
                    $(".sameheight").height("auto");
                }
            });
            $(window).on("resize", function () {
                if ($(window).width() > 768) {
                    equalheight(".sameheight");
                }
                else {
                    $(".sameheight").height("auto");
                }
            });
</script>
</div></span></span>
        </footer>
    </div>
</div>
                    <input type="hidden" name="__antiCSRF" id="__antiCSRF" value=""/>
                </form>
            </div>
        </div>
    </div>
    
    <script type="text/javascript" src="/js/anti-csrf.js">
    </script>
