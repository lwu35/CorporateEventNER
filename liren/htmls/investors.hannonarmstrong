                exchangeLabel: 'NYSE',
                symbolLabel: 'HASI',
            },
        ],
        // commonly used in Stock Charts and Dividend Charts
        chartColors: {
            baseColor: '8, 114, 162', //only in rgb format
            specialColor: '26, 188, 156' //only in rgb format
        },
        isHeaderSticky: false,
        isSecondaryNavSticky: false,
    }
</script>
