            '{{#years}}' +

                    '{{#items}}' +
                        '{{#docs}}'+
                        '<a href="{{docUrl}}" target="_blank"> ' +
                        '<img width="232" height="300" src="{{docThumb}}" class="image wp-image-4526  attachment-medium size-medium" alt="" ' +
                        'style="max-width: 100%; height: auto;" srcset="{{docThumb}} 232w, {{docThumb}} 400w, {{docThumb}} 500w" sizes="(max-width: 232px) 100vw, 232px"> ' +
                    '</a> ' +
                        '{{/docs}}'+
                    '{{/items}}' +
            '{{/years}}'
            // @formatter: on
            /* beautify preserve:end */
    ),beforeRender: function(e, data){
    //   console.log(data)
    },
    beforeRenderItems: function(e, data) {
        $.each(data.items, function(idx, item) {
            $.each(item.docs, function(idx, doc) {
                // Make sure any local links open in the same window
                var url = doc.docUrl.toLowerCase().split('.');
                if (url[url.length - 1] != 'aspx') {
                    doc.blank = true;
                }
            });
        });
    }
});
</script>


<script type="text/javascript">
    $('.annual_report').financials({
        usePublic: GetViewType() != "0",
