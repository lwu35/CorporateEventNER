    limit: 3,
    dateFormat: 'M dd, yy',
    itemContainer: '.module_container--content',
    itemTemplate: (
        '<div class="module_item-box border border--violet">'+
            '<div class="item-box--wrap background--grey">'+
                '<div class="skew-right background--violet dark skew-box-text">Press Release</div>'+
                '<div class="item-box_content">'+
                    '<div class="item-box_content-wrap">' +
                        '<div class="item_date module_date-time ">' +
                            '<span class="module_date-text">{{date}}</span>' +
                        '</div>'+
                        '<h4 class="item_headline module_headline">{{title}}</h4>'+
                        '<a href="{{url}}" class="item_link module_link module_link-learn-more">Read More</a>' +
                    '</div>' +
                '</div>'+
                '<div class="item-box_image item-box_image-background">'+
                        '<div class="item_image item_image-news">'+
                            // '<img src="{{thumb}}" alt="{{title}}">'+
                        '</div>'+
                '</div>'+
            '</div>'+
        '</div>'
    ),
    beforeRenderItems: function(e, data) {
        $.each(data.items, function(idx, item) {

            if ( !item.thumb.length ) {
                if (item.multimedia.length) {
                    item.thumb = item.multimedia[0].url;
                }
            }

            var url = item.url.toLowerCase().split('.');
            if (url[url.length - 1] == 'pdf') {
                item.blank = true;
            }
        });
    },
    itemsComplete: function(e) {
        var  _ = $(e.target);

        _.find('.module_pager-news').pager({
            content: _.find('.module_container--content > .module_item-box'),
            perPage: 1,
            append: false,
            pageMessageContainer: '.module_pager-news .pager-page',
            pageMessageTemplate: '{{page}} / {{pageCount}} ',
            showPages: true,
            pageRange: 1,
            showFirstLast: false,
            showPrevNext: true,
            labels: {
                prev: '',
                next: '',
            },
        });
    }
});


// Past Event Widget
$('.module-event-past .module_container--widget').events({
    usePublic: GetViewType() != "0",
    apiKey: Q4ApiKey,
    showAllYears: true,
