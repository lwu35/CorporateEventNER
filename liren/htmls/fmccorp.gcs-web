        'First Quarter': '1st Quarter',
        'Second Quarter': '2nd Quarter',
        'Third Quarter': '3rd Quarter',
        'Fourth Quarter': '4th Quarter'
    },
    limit: 1,
    append: false,
    loadingMessage: '<p class="module_loader"><i class="q4-icon_spinner"></i><span class="sr-only">Loading latest financials...</span></p>',
    template: (
        /* beautify preserve:start */
        '{{#items}}' +
            '<div class="module_item-box border border--orange">' +
                '<div class="item-box--wrap background--grey">'+
