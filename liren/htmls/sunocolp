    <!-- Cookie notification at bottom of page -->
    <script>
        function elegantPromo(a) {
            function l(a, b) {
                return "undefined" == typeof a ? b : a;
            }
            function m(a) {
                var b = 24 * a * 60 * 60 * 1e3,
                    c = new Date();
                return c.setTime(c.getTime() + b), "; expires=" + c.toGMTString() + "; path=/";
            }
            function n(a, d, e) {
                (document.cookie = a + "=; expires=-1; path=/"), "false" === e ? (document.cookie = a + "=" + d + "; path=/") : "long" === e ? (document.cookie = a + "=" + d + c) : (document.cookie = a + "=" + d + b);
            }
            function o() {
                jQuery("head").append(e),
                    jQuery("body").prepend(d),
                    jQuery(".promo-slide-in-2-button").on("click", function () {
                        p();
                    }),
                    jQuery(".btn-accept").on("click", function () {
                        p(),
                            jQuery(".promo-slide-in-2")
                                .addClass("promo-slide-in-2-closed")
                                .delay(1e3)
                                .queue(function (a) {
                                    jQuery(this).hide(), a();
                                });
                    });
            }
            function p() {
                (new_value = "closed"), n(i, new_value, "true");
            }
            function r() {
                var a = document.cookie.split("; ").reduce(function (a, b) {
                    var c = b.split("=");
                    return (a[c[0]] = c[1]), a;
                }, {});
                return a;
            }
            function s(a) {
                var b = "; " + document.cookie,
                    c = b.split("; " + a + "=");
                if (2 == c.length) return c.pop().split(";").shift();
            }
            var a = a || {},
                b = m(a.cookieExpire) || "",
                c = m(a.longExpire) || "",
                d = l(a.promohtml, ""),
                e = l(a.promocss, ""),
                h = (l(a.promopage, ""), document.getElementsByTagName("html")[0], r()),
                i = "sunoco-lp-cookie-consent",
                k = (h[i], s("sunoco-lp-cookie-consent"));
            if (!k) {
                (base_value = "open"), (document.cookie = i + "=" + base_value + b);
                var k = s("sunoco-lp-cookie-consent");
            }
            "open" === k && o();
        }
        var _elegantPromo = elegantPromo({
            cookieExpire: 30,
            longExpire: 365,
            promopage: "gallery",
            promocss:
