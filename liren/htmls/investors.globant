					$(".wd_recaptcha2_needed").each(function() {
						grecaptcha.render(this, {"sitekey": "6LdDmR8TAAAAAJ72DH_RtavsrDUlu6g6lzvajKsK"});
						$(this).removeClass("wd_recaptcha2_needed");
					});
				}
