			$(this).prepend('<div class="wd_event-teaser-image" style="background-image:url(images/zoom-02.jpg)"></div>');
		} else {
			$(this).prepend('<div class="wd_event-teaser-image" style="background-image:url(images/zoom-05.jpg)"></div>');
		}
		$(this).append('<div class="wd_moreinfo"><a href="'+eventLink+'">MORE INFO</a></div>');
		$(this).find('.item_date,.item_time').wrapAll('<div class="wd_eventdatetime"></div>');
		$(this).find('.wd_eventdatetime').insertBefore($(this).find('.item_name'));
		$(this).find('.wd_event_subscribe').appendTo($(this).find('.wd_eventdatetime'));
		$(this).find('.wd_event_reminder').appendTo($(this).find('.wd_eventdatetime'));
	});

	
	// Color News Category Links
	$('.wd_newsfeed_releases .wd_category_link a').each(function(){
		var linkSplits = $(this).attr('href').split('=');
		if(linkSplits[1] == 1) $(this).css('color','purple');
		if(linkSplits[1] == 2) $(this).css('color','#028484;');
	});

	// $('.wd_newsfeed_releases .wd_title').text(
	// 	$('.wd_newsfeed_releases .wd_title').text().substr(0, 75)+'...'
	// );

	$('button.navbar-toggler').click(function(){
		$(this).parent().toggleClass('wd_visibleoverflow');
	});

	// Sec Filings moving GLOB
	$('.wd_ir_secfilings .wd_filer_label').insertAfter($('.wd_ir_secfilings .wd_selectors'));

	//Quarterly Earnings 
	//This is responsible for wrapping the attachment container and whatever "embed code" is place within in item as an additional attachment
	$('.wd_pageid_126 .item').each(function() {
		$(this).find('.wd_attachment').wrapAll('<div class="wd_attachment_container"></div>');
	});
	//This makes it so that way any earnings release item is clicked, the attchment information is shown or hidden
	$('.wd_pageid_126 .item').click(function() {
		$(this).find('.wd_attachment_container').slideToggle();
		$(this).find('.item_name').toggleClass('opened');

	});
	//For the items on the webinars page this takes the images from each item and make it a background image
	$('.wd_pageid_97 .item').each(function() {
		var webinarImage = $(this).find('.wd_imagetable img').attr('src');
		$(this).prepend('<div class="webinar_image_container" style="background-image: url('+ webinarImage+')"></div>');
		$(this).find('.wd_imagetable').remove();
	});

	//This wraps the item name, attachment, and paragraphs within each item on the webinars page in order to make a border around all these elements.

	$('.wd_pageid_97 .item').each(function() {
		$(this).find('.item_name, .webinar_pink_date, .webinar_pink_button, .webinar_grey_date, .webinar_grey_button, .webinar_description, .wd_attachment').wrapAll('<div class="webinar_info_border"></div>');
	});
});
</script>
