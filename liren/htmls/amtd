        </div>
    </div>
</div></span><span class='Q4FooterDiv23'><div id="_ctrl0_ctl60_divModuleContainer" class="module module-embed hidden">
    <div class="module_container module_container--outer">
        
        <div class="module_container module_container--inner">
            <script>
var q4App = $.extend(true, q4Defaults, {
    options: {
        headerOffset: $('.pane--header').outerHeight()
    },
    // toggle contrast function
    contrast: {
        toggle: '.module-contrast_button',
        getCookie: function() {
            return document.cookie.replace(/(?:(?:^|.*;\s*)contrast\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        },
        setCookie: function(boolean) {
            document.cookie = 'contrast=' + boolean + '; path=/';
        },
        init: function() {
            var inst = this;
            if (!inst.getCookie().length) {
                inst.setCookie('false');
            } else if (inst.getCookie() == 'true') {
                $(inst.toggle).addClass('js--active');
                $('body').addClass('js--contrast');
            }
            $(inst.toggle).on('click', function(e) {
                e.preventDefault();
                if (inst.getCookie() == 'false') {
                    inst.setCookie('true');
                } else {
                    inst.setCookie('false');
                }
                $(this).toggleClass('js--active');
                $('body').toggleClass('js--contrast');
            });
        }
    },
    scollingTable: function(selector) {
        $(selector).not(selector + ' table').wrap('<div class="table-wrapper" />');
    },
    sections: function() {
        var hash = decodeURIComponent(window.location.hash.length ? window.location.hash.substring(1) : '');
        if ($('.module-' + hash).length) {
            setTimeout(function() {
                q4App.scrollTo($('.module-' + hash));
                window.location.hash = hash;
            }, 1000);
        }
        $('nav').on('click', 'a[href*="#"]', function(e) {
            var $parent = $(this).closest('ul').parent('li'),
                hash = $(this).attr('href').split('#')[1];
            if ($parent.hasClass('selected')) {
                e.preventDefault();
                window.location.hash = hash;
                q4App.scrollTo($('.module-' + hash));
            }
        });
    },
    //sticky header 
    stickyHeader: function() {
        // Hide Header on on scroll down
        var didScroll;
        var lastScrollTop = 0;
        var delta = 5;
        var headerHeight = $('.pane--header').outerHeight();
        $(window).on('scroll', function(event) {
            didScroll = true;
        });
        setInterval(function() {
            if (didScroll) {
                hasScrolled();
                didScroll = false;
            }
        }, 250);

        function hasScrolled() {
            var st = $(this).scrollTop();
            if (Math.abs(lastScrollTop - st) <= delta)
                return;
            if (st > lastScrollTop && st > headerHeight) {
                $('.layout').removeClass('js--header-down').addClass('js--header-up');
                $('.pane--header').css('top', -headerHeight);
            } else {
                if (st + $(window).height() < $(document).height()) {
                    $('.layout').removeClass('js--header-up').addClass('js--header-down');
                    $('.pane--header').removeAttr('style');
                }
            }
            lastScrollTop = st;
        }
    },
    //header functionality
    headerMenusToggle: function() {
        $('.module-header-social-button_toggle-layout').on('click', function() {
            $('.layout').toggleClass('js--mobile');
            $('.layout').removeClass('js--stock-active');
            $('.layout').removeClass('js--search-active');
        });
        $('.module-header-social-button_search').on('click', function() {
            $('.layout').toggleClass('js--search-active');
            $('.layout').removeClass('js--mobile');
            $('.layout').removeClass('js--stock-active');
        });
        $('.module-stock-header-mini .module_container--inner').on('click', function() {
            $('.layout').toggleClass('js--stock-active');
            $('.layout').removeClass('js--mobile');
            $('.layout').removeClass('js--search-active');
        });
        $('.module-header-close_button').on('click', function() {
            $('.layout').removeClass('js--stock-active');
            $('.layout').removeClass('js--mobile');
            $('.layout').removeClass('js--search-active');
        });
    },
    //appending footer links top into the footer navigation
    appendQuickLinks: function() {
        $('.module-links-footer-top').appendTo('.nav--footer .level1').addClass('js--module-append').wrap('<li class="js--append"/>');
    },
    //adding a class on search input when has value
    searchVal: function() {
        $('.module-search_input').on('keyup', function() {
            if ($('.module-search_input').val().length > 0) {
                $(this).addClass('js--has-value');
            } else {
                $(this).removeClass('js--has-value');
            }
        });
    },
    //appending footer links top into the footer navigation
    appendPopularSearches: function() {
        var elements = $(".module-links-searches ul > li");
        for (var i = 0; i < elements.length; i += 3) {
            elements.slice(i, i + 3).wrapAll("<div class='js--module-links_list-item'></div>");
        }
        $('.module-links-searches').appendTo('.module-search .module_container--outer').addClass('js--module-append');
    },
    //custom navigation functionality
    customNav: function() {
        $('.nav--main li.has-children').prepend('<span class="nav--main_toggle q4-icon_downtick-small"></span>');
        $('.nav--main li.has-children').append('<span class="nav--main_border"></span>');
        $('.nav--main').on('click', 'li.has-children:not(.home) > .nav--main_toggle', function() {
            var $this = $(this),
                $parent = $this.parent();
            if (!$parent.hasClass('js--expanded')) {
                $parent.siblings().removeClass('js--expanded');
                $parent.addClass('js--expanded');
            }
        });
        $('.nav--main').on('click', '.nav--main_border', function() {
            $(this).parent().removeClass('js--expanded');
        });
    },
    init: function() {
        var app = this;
        app.cleanUp();
        app.submitOnEnter('.module-unsubscribe');
        app.submitOnEnter('.module-search');
        app.validateSubmit('.module-search');
        app.cleanQuickLinks($('.module-links'));
        app.copyright($('.copyright_year'));
        app.docTracking();
        app.fancySignup();
        app.resetDate(['.nav a[href*="s3.q4web.com"]:not([href$=".pdf"])']);
        app.previewToolbar();
        app.sections();
        app.stickyHeader();
        app.headerMenusToggle();
        app.customNav();
        app.appendQuickLinks();
        app.searchVal();
        app.appendPopularSearches();
        // app.contrast.init();
    }
});
q4App.init();
$(window).on('load resize', function() {
    if ($(window).width() < 768) {
        q4App._onMobileMenuExpand($('.nav--footer'));
    }
});
</script>
        </div>
    </div>
</div></span></div>
            </div>
        </div>
    </div>
