    $('.module-event-recent-upcoming .module_container--wrapper').events({
        usePublic: GetViewType() != "0",
        apiKey: Q4ApiKey,
        useMoment: true,
        dateFormat: {
            date: 'MMM D, YYYY',
            time: 'hh:mm A'
