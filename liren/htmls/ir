        itemsComplete: function(e) {
            var _ = $(e.target);
            _.find('.module_pager').pager({
                content: $(e.target).find('.module_item'),
                perPage: 7,
                append: false,
                showFirstLast: false
            });
            if (_.find('.module_pager > .pager-page').length <= 1) {
                _.find('.module_pager').hide();
            } else {
                _.find('.module_pager').show();
            }
        }
