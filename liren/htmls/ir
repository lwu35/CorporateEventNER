                '<div id="SubscriberConfirmation" class="module module-subscribe module-subscribe--fancy module-subscribe--footer grid_col grid_col--2-of-4 grid_col--lc-1-of-1 ">' +
                '   <div class="module_container--outer">' +
                '       <h2 class="module_title">Email Alerts</h2>' +
                '       <div class="module_container--inner">' +
                '           <p class="module_message module_message--success"></p>' +
                '       </div>' +
                '   </div>' +
                '</div>'
            ),
            location: '.pane--footer2',
            submitText: 'Sign Up'
        },
    },
    scrollingTable: function(selector) {
        $(selector).not(selector + ' table').wrap('<div class="table-wrapper" />');
    },
    meetingFancy: function($selector, trigger) {
        var meetingCookie = sessionStorage.getItem('requestMeeting');
        if (meetingCookie != 'closed') {
            $selector.removeClass('js--hidden');
        }
        $selector.on('click', '.button--cookie', function() {
            $selector.addClass('js--hidden');
            sessionStorage.setItem("requestMeeting", 'closed');
        });
        $selector.on('click', trigger, function(e) {
            e.preventDefault();
            $.fancybox.open({
                type: 'iframe',
                src: '/request-meeting-iframe/default.aspx',
                opts: {
                    smallBtn: false,
                    margin: [10, 0],
                    slideClass: 'fancybox-slide--form',
                    parentEl: 'form'
                }
            });
        });
        window.addEventListener('message', function(event) {
            if (event.data == 'close-fancybox') {
                $.fancybox.close();
            }
        }, false);
    },
    irMobileNav: function() {
        var $body = $('body');
        $('.nav--secondary .level1').clone().appendTo($('.nav--mobile-ir'));
        $('.module-investor-mobile-navigation .module_title').on('click', function() {
            if (!$body.hasClass('js--ir-nav-opened')) {
                $body.addClass('js--ir-nav-opened').find('.nav--mobile-ir').slideDown();
            } else {
                $body.removeClass('js--ir-nav-opened').find('.nav--mobile-ir').slideUp();
            }
        });
        $('.nav--mobile-ir .level1').on('click', 'li.has-children > a', function(e) {
            if (!$(this).closest('li').hasClass('js--expanded')) {
                e.preventDefault();
                $(this).closest('li').addClass('js--expanded').find('>ul').slideDown();
                $(this).closest('li').siblings().removeClass('js--expanded').find('>ul').slideUp();
            }
        });
    },
    corporateMobileNav: function() {
        var $body = $('body');
        $('.nav--corporate .level1').clone().prependTo($('.pane--breadcrumb .pane_inner')).wrap('<nav class="nav nav--mobile-corporate" />');
        $('.layout_toggle').on('click', 'i', function() {
            $body.toggleClass('js--mobile');
        });
    },
    stickyNavBar: function() {
        var inst = this,
            $window = $(window),
            $layout = $('.layout'),
            $header = $('.pane--header'),
            $nav = $('.pane--navigation .pane_inner-wrapper'),
            $navContainer = $('.pane--navigation'),
            navContainerPos = function() {
                return $navContainer.position().top;
            };

        $window.on('scroll', function() {
            if ($window.scrollTop() + $header.outerHeight() >= navContainerPos()) {
                $layout.addClass('js--sticky');
                $nav.css('top', $header.outerHeight());
            } else {
                $layout.removeClass('js--sticky');
                $nav.removeAttr('style');
            }
        });
        $window.on('resize', function() {
            $layout.removeClass('js--sticky');
            $nav.removeAttr('style');
            $window.trigger('scroll');
        }).resize();
    },
    backToTopButton: function() {
        $('.module-back-to-top').on('click', '.module-back-to-top_button', function() {
            q4App.scrollTo($('.layout'));
        });
    },
    q4Credits: function() {
        $('.module-q4-credits .module-q4-credits-link').appendTo($('.module-q4-credits--apended'));
    },
    init: function() {
        var app = this;

        app.cleanUp();
        app.submitOnEnter('.module-unsubscribe');
        app.submitOnEnter('.module-search');
        app.validateSubmit('.module-search');
        app.superfish($('.nav--secondary .level1'), { cssArrows: false });
        app.cleanQuickLinks($('.module-links'));
        app.copyright($('.copyright_year'));
        app.reveal('.pane--footer2 .module-subscribe', '.module_input', '.module-subscribe_mailing-list', true);
        app.docTracking();
        app.fancySignup();
        app.resetDate(['.nav a[href*="s4.q4web.com"]:not([href$=".pdf"])']);
        app.previewToolbar();
        app.sections();
        app.contrast.init();
        app.meetingFancy($('.module-cta'), '.button--meeting');
        app.stickyNavBar();
        app.irMobileNav();
        app.corporateMobileNav();
        app.backToTopButton();
        app.q4Credits();
    }
});

q4App.init();
</script>
        </div>
    </div>
</div></span></div>
            </div>
        </div>
    </div>
</div>
                    <input type="hidden" name="__antiCSRF" id="__antiCSRF" value=""/>
                </form>
            </div>
        </div>
    </div>
    
