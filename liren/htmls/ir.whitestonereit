            <div class="container inner-page">
                <div id="irwWrapper">
                    <span class='ContentPaneDiv'><span class='ContentPaneDiv1'><div id="_ctrl0_ctl33_divModuleContainer" class="module module-embed module--no-padding-top">
    
    <div class="container presentations">
    <div id="PresentationDocsform">
        <div class="row">
            <div class="col-xs-12">
                <div class="row">
                    <div class="col-xs-12 irwPresentationDocs">
                        <div class="col-xs-12 irwEventAlert">
                            <div class="row">
                                <div class="col-lg-6">
                                    <span class="fa fa-envelope"></span>
                                    <a href="/r/investor-email-alerts/"><span>Notify me of new presentations posted to this site.</span></a>
                                </div>
                            </div>
                        </div>
                        <table class="table table-hover irwDocCommonTbl">
                            <thead>
                                <tr>
                                    <th class="bg-primary">
                                        <div class="row">
                                            <div class="col-xs-3 col-sm-2 col-md-2 col-lg-2">Date</div>
                                            <div class="col-xs-6 col-sm-8 col-md-9 col-lg-9">Title</div>
                                            <div class="col-xs-3 col-sm-2  col-md-1 col-lg-1" style="text-align:right">Format</div>
