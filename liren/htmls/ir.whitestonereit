         // used for Stock Splits & Dividends section as well as Basis Calculator
        stockSplitData: [{
                "exDate": "8/24/2010",
                "recordDate": "-",
                "announceDate": "8/24/2010",
                "payDate": "-",
                "type": "reverse stock split",
                "ratio": "1:3"
            }
        ]
    }
</script>
