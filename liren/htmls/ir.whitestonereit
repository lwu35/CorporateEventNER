<script type="text/javascript">
    $('.irwPresentationDocs').presentations({
        usePublic: GetViewType() != "0",
        apiKey: Q4Settings.apiKey,
        showAllYears: true,
        dateFormat: 'm/d/yy',
        itemContainer: 'tbody',
        beforeRender: function(e, data) {
          	data.items = $.each(data.items, function(i, item) {
                if(!item.docUrl) {
                    item.docUrl = item.url;
                }
            });
        },
        itemTemplate: (
            /* beautify preserve:start */
            // @formatter:off
            '<tr class="text-primary irwHasGA"><td>' +
                '<div class="row">' +
                    '<div class="text-muted irwDocCommonDate  col-xs-3 col-sm-2 col-md-2 col-lg-2">{{date}}</div>' +
                    '<div class="text-primary irwDocCommonLink col-xs-7 col-sm-9 col-md-9 col-lg-9">' +
                        '<a href="{{docUrl}}" title="Select to view the presentation" class="irwGaLabel irwEventTracking" target="_blank">{{title}}</a>' +
                    '</div>' +
                    '<div class="text-muted irwDocCommonFormat col-xs-2 col-sm-1 " style="text-align:right">' +
                        '<a class="irwFilingDownload irw_PDF irwGaHasFile pull-right" data-gafid="{{url}}" href="{{docUrl}}" target="_blank">' +
                            '<i class="fa fa-file-pdf-o irwEventTracking irwGaHasTitle " data-gatitle="PDF" title="PDF"></i>' +
                        '</a>' +
                    '</div>' +
                '</div>' +
            '</td></tr>'
            // @formatter:on
            /* beautify preserve:end */
        ),
        loadingMessage: (
            /* beautify preserve:start */
            // @formatter:off
            '<tr class="text-primary">' +
                '<td class="irwLoadingSpinner"><i class="fa fa-spinner fa-spin"></i><span class="sr-only">Loading ...</span></td>' +
            '</tr>'
            // @formatter:on
            /* beautify preserve:end */
        ),
        itemLoadingMessage: (
            /* beautify preserve:start */
            // @formatter:off
            '<tr class="text-primary">' +
                '<td class="irwLoadingSpinner"><i class="fa fa-spinner fa-spin"></i><span class="sr-only">Loading ...</span></td>' +
            '</tr>'
            // @formatter:on
            /* beautify preserve:end */
        )
    });
</script>
</div></span></span>
