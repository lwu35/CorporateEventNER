                    function onRequestStart(sender, args) {
                        //alert(args.get_eventTarget());
                        if (args.get_eventTarget().indexOf("ExportToExcelButton") >= 0 ||
                            args.get_eventTarget().indexOf("ExportToWordButton") >= 0 ||
                            args.get_eventTarget().indexOf("ExportToPdfButton") >= 0 ||
                            args.get_eventTarget().indexOf("rtb_ResCompareManagement_pgComparisonSetup") >= 0 ||
                            args.get_eventTarget().indexOf("rtb_ResCompareManagement_pgHotelToSystem") >= 0 ||
                            args.get_eventTarget().indexOf("rtb_ResCompareManagement_pgSystemToHotel") >= 0) {                            
                            args.set_enableAjax(false);
                        }
                        else {                    
                            var viewportHeight = document.documentElement.clientHeight;
                            $get("RadAjaxLoadingPanel1").style.height = (viewportHeight - 70) + "px";
                        }

                        if (typeof onRequestStartLocal === 'function') {
                            onRequestStartLocal(sender, args);
                        }
                    }

                    function setVariablesMaster() {
                        
                    }
                    
                    $(function () {                    
                        if (typeof loadLocal === 'function') {
                            loadLocal();
                        }
                    });


                    function onRequestEnd(sender, args) {
                    
                        if (typeof setVariables === 'function') {
                            setVariables();
                        }
                
                        if (typeof onRequestEndLocal === 'function') {
                            onRequestEndLocal(sender, args);
                        }

                        if (typeof GridCreated === 'function') {
                            GridCreated();
                        }

                        ResizePage();
                        ReDisplayHeader();
                    }

                    $(window).bind('resize', function () {
                    
                        if (typeof GridCreated === 'function') {
                            GridCreated();
                        }

                        if (typeof bindLocal === 'function') {
                            bindLocal();
                        }
                    });

                    function OnClientPageLoadRadWindow(sender, args) {
                        //See Indoor Classic
                    }

                    function onClientClose(oWnd, args) {
                        //See Indoor Classic
                    }


