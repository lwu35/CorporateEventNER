            <script type="text/javascript">
var q4App = $.extend(q4Defaults, {
    init: function() {
        var app = this;

        app.cleanUp();

        app.unWrapLink('a.StockPrice');
        app.submitOnEnter('.ModuleSearch');
        app.submitOnEnter('.MailingListUnsubscribeContainer');
        app.validateSubmit('.ModuleSearch');
        app.accessibleNav($('.navbar-collapse'), 'level1');
        app.onMenuToggle();
        app.onMenuExit($('.PaneNavigation .menu-close'));
        app.appendSearch();
        app.addFixedClass();
        app.toggleCorpNav();
        app.scrollConfirmation();
        app.appendBreadcrumb();
        app.mobileNavToggle();
        app.onMobileMenuChildrenExpand();
        app.changeClass();
        //app.scroll_to_section_menu('.mainNav', '.level2', '.pageSection-', true, '.PaneHeader', true, false);
        //app.scroll_to_section_menu('.mobileNav', '.level2', '.pageSection-', true, '.PaneHeader', true, true);

        $('.PreviewToolBar').previewToolbar();
        $('.MailingListSignupContainer').captcha({
            submitText: "Sign Up",
            submitTextFancy: "Sign Up",
        });
    },
    initScroll: function() {
        var app = this;
        //app.stickyNav('.PaneBreadcrumb', '.stickyNav', '.PaneHeader', 'navStuck');
    },

    //Appending search module to footer html
    appendSearch: function() {
        $('.ModuleSearch').appendTo($('.footerHtml .footerSearch'));
    },

    //Adding class to LayoutDefault for sticky header
    addFixedClass: function() {
        $(window).scroll(function() {
            var scroll = $(window).scrollTop();

            if (scroll >= 5) {
                $(".LayoutDefault").addClass("sticky");
            } else {
                $(".LayoutDefault").removeClass("sticky");
            }
        }).scroll();
    },

    //Corporate Navigation toggle
    toggleCorpNav: function() {
        $('.corpNavMobile .collapse-button').on('click', function() {
            $(this).toggleClass('active').next().slideToggle();
        });
    },

    //Appeding breadrcumb links from corporate
    appendBreadcrumb: function() {
        $('<a href="https://schneider.com">Home</a><span class="BreadcrumbSeparator">&gt;</span>').prependTo($('.Breadcrumb .ModuleInnerContainer'));
    },

    //Changing the class on level1 li
    changeClass: function() {
        $('.LayoutHomePage .mainNav .level1 > li').removeClass('expanded').addClass('selected');
        $('.LayoutHomePage .mobileNav .level1 > li').removeClass('expanded').addClass('selected');
    },
    scrollConfirmation: function() {
        $(window).load(function() {
            if ($('.EditSubscriberConfirmation').text().length) {
                $('html,body').animate({
                    scrollTop: $('.EditSubscriberConfirmation').offset().top - 150
                });

            }
        });
    },
    mobileNavToggle: function() {
        $('.mobileNav .level2 li:first-child').on('click', function(e) {
            var $this = $(this),
                $parent = $('.mobileNav');
            if (!$parent.hasClass('open')) {
                e.preventDefault();
                $parent.addClass('open');
                $parent.find('.open').removeClass('open');
            }
        });
        $('.LayoutDefaultInner').on('click', function(e) {
            !$(e.target).closest('.mobileNav').length ? $('.mobileNav').removeClass('open') : null;
        });
    },

    onMobileMenuChildrenExpand: function() {
        $('.mobileNav').on('click', 'li.has-children > a', function(e) {
            if ($(document).width() < 1024) {
                var $this = $(this),
                    $parent = $this.parent();
                if (!$parent.hasClass('open')) {
                    e.preventDefault();
                    $parent.addClass('open').find('>ul').slideDown();
                    $parent.siblings().removeClass('open').find('>ul').slideUp();
                    $parent.siblings().find('.open').removeClass('open').find('ul').hide();
                }
            }
        });
    },
    scroll_to_section_menu: function(menu, level, sectionClass, addActiveClass, offsetClass, menuIsSicky, closeMobileNav) {
        //===========
        //** menu = string - navigation css class
        //
        //** level = string - navigation level css class that has as children the scrolling links.
        //
        //** sectionClass = string - Common part of the class that is used on all modules on the long page e.g. ".pageSection-" used on a module as ".pageSection-moduleName"; Same "moduleName" will be used on link as "?section=moduleName"
        //
        //** addActiveClass = boolean - "true" to add active class on link for cureent module that was scrolled to.
        //
        //** offsetClass = string - css class of sticky element at the top, usually a navigation; offset will be the height of "offsetClass" element.
        //
        //** menuIsSicky = boolean - "true" if scrooling menu is sticky to add scroll offset for itself.
        //
        //** closeMobileNav = boolean - "true" to close mobile nav after scroll ***works only with default mobile navigation functionality
        //To work needs the following methods: _scroll_to_section, _scroll_to_sec_load, _make_section_active, _prevent_make_section_active.
        //===========
        var _ = this;

        _._scroll_to_section(menu, level, offsetClass, sectionClass, closeMobileNav, menuIsSicky);

        $(window).on('load', function() {
            _._scroll_to_sec_load(menu, offsetClass, sectionClass);
        });
        if (addActiveClass === true) {
            $(window).on('scroll', function(e) {
                _._make_section_active(menu, level, offsetClass, sectionClass);
            }).scroll();
        }
    },
    _prevent_make_section_active: false,
    _scroll_to_section: function(menu, level, offsetClass, sectionClass, closeMobileNav, menuIsSicky) {
        var _ = this;
        $(menu + " .selected > " + level + ' > li > a[href*="?section="]').on('click', function(e) {
            var section = sectionClass + $(this).attr("href").split('section=').pop(),
                offset = $(offsetClass).length ? $(offsetClass).outerHeight() : "";
            offset = menuIsSicky ? offset + $(menu).outerHeight() : offset;
            e.preventDefault();
            //prevent make_section_active function to run while page is animating scroll when the links are clicked
            _._prevent_make_section_active = true;
            $('html,body').animate({
                scrollTop: $(section).offset().top /*- parseInt($(section).find('h2').css('margin-top'))*/ - offset
            }, 400, function() {
                if (!$(this).is('animated')) {
                    //restore make_section_active fucntion to run when the animated scroll stopped
                    _._prevent_make_section_active = false;
                }
                closeMobileNav == true ? $('.LayoutDefaultInner').triggerHandler('click') : null;

            });
            $(this).parent('li').siblings().removeClass('js_active');
            $(this).parent().addClass('js_active');
        });

    },
    _make_section_active: function(nav, level, offsetClass, sectionClass, menuIsSicky) {
        //only run if physical scroll and not on animation scroll achived by changing _prevent_make_section_active value in _scroll_to_section function.
        if (this._prevent_make_section_active == false) {
            var curentScroll = $(offsetClass).length ? $(window).scrollTop() + $(offsetClass).outerHeight() : "",
                sectionClass = sectionClass.substr(1), //remove point from begining
                $pageSections = $('[class*="' + sectionClass + '"]');
            curentScroll = menuIsSicky ? curentScroll + $(nav).outerHeight() : curentScroll;

            $pageSections.each(function() {
                var $this = $(this),
                    scrollToTop = $this.offset().top /*- parseInt($this.find('h1').css('margin-top'))*/ ,
                    menuItems = $(nav + " .selected > " + level + ' > li');
                if (curentScroll >= scrollToTop) {
                    var section = $this.attr("class").split(sectionClass).pop().split(" ").shift(),
                        activeItemParent = $(nav).find('a[href$="' + section + '"]').parent();
                    menuItems.removeClass('js_active');
                    activeItemParent.addClass('js_active');
                }
            });
        }
    },
    _scroll_to_sec_load: function(nav, offsetClass, sectionClass, menuIsSicky) {
        if (location.href.indexOf('section=') > -1) {
            var section = sectionClass + location.href.split('section=').pop(),
                offset = $(offsetClass).length ? $(offsetClass).outerHeight() : "";
            offset = menuIsSicky ? offset + $(nav).outerHeight() : offset;

            $('html,body').animate({
                scrollTop: $(section).offset().top /*- parseInt($(section).find('h2').css('margin-top'))*/ - offset
            });
        }
    },
    stickyNav: function(nav, level, offsetTop, stikyCls) {
        // offsetTop - class of and element that is already fixed at the top of the page(e.g. fixed corporate nav)
        if ($(nav).length) {
            var CurentScroll = $(window).scrollTop(),
                StickyNav = $(nav + " " + level),
                StickyNavHeight = StickyNav.outerHeight(),
                StickyNavWrapper = $(nav),
                PositionTop = $(offsetTop).length ? StickyNavWrapper.offset().top - parseInt($(offsetTop).outerHeight()) : StickyNavWrapper.offset().top;
            if (CurentScroll >= PositionTop) {
                StickyNavWrapper.css("height", StickyNavHeight);
                StickyNav.addClass(stikyCls);
            } else {
                StickyNav.removeClass(stikyCls);
                StickyNavWrapper.removeAttr("style");
            }
        }
    }
});

$(document).ready(function() {
    q4App.init();
});
$(window).on('scroll', function(e) {
    q4App.initScroll();
}).scroll();
</script>

