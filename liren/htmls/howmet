            <script type="text/javascript">

                //set javascript variables to hold the IDs of consent groups
                var strOneTrustClosedCookieName = "OptanonAlertBoxClosed";
                var strOneTrustStrictlyNecessaryID = "1";
                var strOneTrustPerformanceID = "2";
                var strOneTrustFunctionalID = "3";
                var strOneTrustTargetingID = "4";

                //extra javascript values for information capturing
                var strOneTrustFramePlaceholder = "/global/en/general/onetrust-frame-placeholder.asp";
                var strOneTrustBase = "optanon-category-";

                //regexes for checking cats accepted in OptanonActiveGroups var
                var regCheckStrictlyNecessary = /,1,/; 
                var regCheckPerformance = /,2,/; 
                var regCheckFunctional = /,3,/; 
                var regCheckTargeting = /,4,/; 

                //set a javascript object to track the acceptance of various cookie consent status
                var boolCookieConsentArray = {};
                boolCookieConsentArray[strOneTrustClosedCookieName] = false;
                boolCookieConsentArray[strOneTrustStrictlyNecessaryID] = false;
                boolCookieConsentArray[strOneTrustPerformanceID] = false;
                boolCookieConsentArray[strOneTrustFunctionalID] = false;
                boolCookieConsentArray[strOneTrustTargetingID] = false;

                //set a javascript variable to control if banner should slide page down
                var boolOneTrustSlidePageDown = true;

                //define variable to be used across functions
                var topMargin = 0;


                //function that gracefully slides down the html body
                function pushPageDown() {
					
						document.body.style.display = "block";
					

                    var elemOptAnonBanner = document.getElementById('onetrust-banner-sdk');
                    var btnAcceptCookies = document.getElementById("onetrust-accept-btn-handler");
                    var btnOptBannerLinkTxt = document.getElementById("ot-banner-policy-link");
                    //set site primary colors
                    btnOptBannerLinkTxt.style.color = "#00AFBA";
                    btnAcceptCookies.style.backgroundColor = "#00AFBA";
                    btnAcceptCookies.style.borderColor = "#00AFBA";

                    elemOptAnonBanner.style.position = "fixed";
                    elemOptAnonBanner.style.height = "auto";
                    elemOptAnonBanner.style.top = "0px";

                    elemOptAnonBanner.style.transition = "all 1000ms ease-out";
                    elemOptAnonBanner.style.webkitTransition = "all 850ms ease-out";
                    elemOptAnonBanner.style.mozTransition = "all 1000ms ease-out";
                    elemOptAnonBanner.style.msTransition = "all 1000ms ease-out";
                    elemOptAnonBanner.style.OTransition = "all 1000ms ease-out";


                    if (elemOptAnonBanner !== undefined && elemOptAnonBanner !== null && boolOneTrustSlidePageDown) {
                        var bannerHeight = elemOptAnonBanner.offsetHeight;

                        document.body.style.transition = "all 1000ms ease-out";
                        document.body.style.webkitTransition = "all 850ms ease-out";
                        document.body.style.mozTransition = "all 1000ms ease-out";
                        document.body.style.msTransition = "all 1000ms ease-out";
                        document.body.style.OTransition = "all 1000ms ease-out";

                        var elemHeader = document.getElementsByTagName("header")
                        if (elemHeader.length > 0) {
                            for (var i = 0; i < elemHeader.length; i++) {
                                if (window.getComputedStyle(elemHeader[i], null).getPropertyValue("position") != "relative") {
                                    elemHeader[i].style.transition = "all 1000ms ease-out";
                                    elemHeader[i].style.webkitTransition = "all 850ms ease-out";
                                    elemHeader[i].style.mozTransition = "all 1000ms ease-out";
                                    elemHeader[i].style.msTransition = "all 1000ms ease-out";
                                    elemHeader[i].style.OTransition = "all 1000ms ease-out";
                                }
                            }
                        }

                    }
                }

                //function to get cookies by name
                function getCookie(name) {
                    var value = "; " + document.cookie;
                    var parts = value.split("; " + name + "=");
                    if (parts.length == 2) {
                        return true;
                    }
                }

                //wrapper function for OneTrust, runs on page load and banner interaction
                function OptanonWrapper() {
                    if (!getCookie('OptanonAlertBoxClosed')) {
                        pushPageDown();

                        //set privacy page value to link to local policy
                        var elemPrivacyLink = document.getElementById("ot-banner-policy-link");
                        elemPrivacyLink.href = "/global/en/general/privacy.asp?cn=y";
                        elemPrivacyLink.setAttribute("target", "_blank");

						Optanon.OnConsentChanged(function () {
							var elemOptAnonBanner = document.getElementById('onetrust-banner-sdk');
                            elemOptAnonBanner.style.marginTop = "-" + (elemOptAnonBanner.offsetHeight) + "px";

                            document.body.style.transition = "all 1000ms ease-out";
                            document.body.style.webkitTransition = "all 850ms ease-out";
                            document.body.style.mozTransition = "all 1000ms ease-out";
                            document.body.style.msTransition = "all 1000ms ease-out";
                            document.body.style.OTransition = "all 1000ms ease-out";

                            
							document.body.style.display = "block";
							

							document.body.style.marginTop = topMargin + "px";

                            var elemHeader = document.getElementsByTagName("header")

							if (elemHeader.length > 0) {
								for (var i = 0; i < elemHeader.length; i++) {
									elemHeader[i].style.marginTop = topMargin + "px";
								}
							}

                        });

                        window.addEventListener('resize', function () {
                            pushPageDown();
                        });
                    }
                    else {
                        boolCookieConsentArray[strOneTrustClosedCookieName] = true;
                    
                    }
                    
                    
                }

                //function to check if a specific OneTrust cookie group has been set
                function OneTrustAcceptanceCheck(strCheckID) {
                    return boolCookieConsentArray[strCheckID];
                }

            </script>
