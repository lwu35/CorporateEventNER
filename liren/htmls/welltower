    function addCommas(nStr)
    {
        nStr += '';
        var x = nStr.split('.');
        var x1 = x[0];
        var x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }


    // load stock quote

	var CLIENT = CLIENT || {};

	var url = "/wp-content/themes/sage-master/lib/client/stock-quote.php";

    function getPercentChange(change, open) {
        if (open && open > 0) {
            return ((change / open) * 100).toFixed(2);
        } else {
            return "0";
        }
    };

	jQuery.get(url, function(json) {
		var $dom = jQuery("#quote");
		var quote = {};
		if(json && json.data) {
		    for(var i = 0; i< json.data.length; i++){
		        if(json.data[i].symbol === "WELL") {
              quote = json.data[i];
            }
        }
    }

		quote.changeNumber = parseFloat(quote.changeNumber).toFixed(2);
		quote.lastTrade = parseFloat(quote.lastTrade).toFixed(2);
		//quote.percentChange = getPercentChange(quote.Change, quote.Open);
		quote.changeSymbol = quote.changePercent >= 0 ? "+" : "";
		quote.volume = addCommas(quote.volume);

		BOKA.handlebars.draw({
			$dom: $dom,
			id: "features/stock-price",
			preprocessed: true,
			prepend: false,
			context: quote
		});
	});
</script>
						
