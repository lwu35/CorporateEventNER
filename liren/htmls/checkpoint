    function positionLinkBlock(targetContainer) {
        if (targetContainer != null) {
            var strLinkBlock = '<div class="be-related-link-container"><div class="be-label">Related Information</div><ul class="be-list"><li class="be-list-item"><a class="be-related-link" href="https://www.checkpoint.com/press-releases/">Press Releases</a></li><li class="be-list-item"><a class="be-related-link" href="https://www.checkpoint.com/#heaD-PRODUCTS">MITRE Engenuity ATT&amp;CK;® Evaluations Highlight...</a></li><li class="be-list-item"><a class="be-related-link" href="https://www.checkpoint.com/infinity-vision/infinity-portal/">Unified Security Management Platform</a></li></ul></div>';
            targetContainer.insertAdjacentHTML('afterbegin', strLinkBlock);
        }
    }

