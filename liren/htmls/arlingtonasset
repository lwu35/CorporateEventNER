<style type="text/css" wd_script_id="template page:events:system style">
.wd_events_inputs {
	margin: 10px 0;
	overflow: auto;
}
.wd_events_inputs select {
	margin-right: 1ex;
}
ul.wd_events_tabs {
	float: right;
	overflow: hidden;
	margin: 5px 0;
	padding: 0px;
}
li.wd_events_tab {
	float: left;
	display: block;
	margin: 0;
	border: 1px solid #666666;
	padding: 5px;
	cursor: pointer;
	border: 1px solid #E1E1E1;
}
.wd_events_tab_label {
