    <div id="header"><div class="pad"><div id="piperpowerlogo"><a href="/"><img id="logo" src="/images/spacer.gif" alt="Piper Sandler ®"></a></div><a id="hamburger" href="javascript:void(0);" style="display: none;" onclick="ToggleMobileNav();"><img src="/images/spacer.gif" height="48" width="48" id="hamburgericon"></a><div id="navwrapper"><div id="nav_primary"><a id="117" href="/2col.aspx?id=117">Investment Banking</a><a id="119" href="/3col.aspx?id=119">Public Finance</a><a id="5631" href="/2col.aspx?id=5631">Institutional Equities</a><a id="103" href="/2col.aspx?id=103">Research</a><a id="1146" href="/2col.aspx?id=1146">Fixed Income</a><a id="794" href="/2col.aspx?id=794">Private Equity Strategies</a><a id="105" href="/2col.aspx?id=105">About Us</a></div><script language="javascript">
        
var menueffect = 'slide';
var menuoptions = { direction: 'left' };
var menuduration = 500;    
var evalshowdiv = '';
var evalHover = '';
            
$(document).ready(function () {
            
    $('.dropdown_menu').mouseenter(
        function() {
            evalHover = this.id;
            $("#" + this.id.substring(12)).addClass('hoveractive');                                
        });

    $( '#nav_primary a' ).hover(
        function() {
            $('#nav_primary a').removeClass('hoveractive');
            $(this).addClass('hoveractive');
            evalHover = this.id;
            PopMenu(this.id);
        }, 
        function() {
            evalHover = '';
            UnPopMenu('popout_menu_' + this.id);
        }
    );
    
    $( '.sub_menu a' ).hover(
        function() {
            $(this).addClass('hoveractive');
            var side_id = '#side_menu_' + this.id;
            ShowSideNav(this, side_id);
        }, 
        function() {
            
        }
    );
                        
    $('.dropdown_menu').mouseleave(
        function(e) {
            evalHover = '';
            UnPopMenu(e.currentTarget.id);
        }
    )
    
});    
           
function ShowSideNav(submenulink, side_id)
{
    $('.side_menu_container').hide();            
    
    $('.sub_menu a:not(submenulink)').removeClass('hoveractive');
    
    var evalbottom = $(side_id).parent().offset().top + $(side_id).parent().height();
    var evalcol1id = side_id + '_1';
    var evalcol2id = side_id + '_2';

    $(submenulink).addClass('hoveractive');
    
    if ($(side_id).hasClass('split'))
    {
        $(side_id).show();
    }
    else
    {
        $(side_id).addClass('eval');
        $(side_id).show();
        $(evalcol1id + " a").filter(function() {return $(this).offset().top + $(this).height() > (evalbottom - 15) }).appendTo(evalcol2id);
    
        var col1Count = $(evalcol1id + " a").length;
        var col2Count = $(evalcol2id + " a").length;
    
        if (col2Count > col1Count)
        {
            var targetCol1Count = Math.ceil((col1Count + col2Count) / 2);
            var itemsToMove = targetCol1Count - col1Count;
            $(evalcol2id + " a").slice(0,itemsToMove).appendTo(evalcol1id);
        }
    
        $(side_id).removeClass('eval');
        $(side_id).addClass('split');
    
    }
    
}

function PopMenu(menuid)
{
    var divid = '#popout_menu_' + menuid;       
    evalshowdiv = divid;
    var previouslyshowncount = $('#popoutcontainer div[id^="popout_menu_"]:not(' + divid + '):visible').length;
        
    $('#popoutcontainer div[id^="popout_menu_"]:not(' + divid + ')').stop(true,true);
    if (divid != evalshowdiv){return false;}
    
    if ($(divid).children('div.side_menu_container').length == 0) {return false;}
                    
    if (evalHover == divid || evalHover == menuid)
    {
        $('#popoutcontainer div[id^="popout_menu_"]:not(' + divid + '):visible').hide();
  
        if (previouslyshowncount > 0)
        {
            $(divid).stop(true,true).show();
        }
        else
        {
            $(divid).stop(true,true).show(menueffect, menuoptions, menuduration);
        }     
        showActiveSideNav(divid);
    }
                
    return false;
}

function showActiveSideNav(divid)
{
    if ($(divid + " .sub_menu .active").length == 1)
    {
        var submenulink = $(divid + " .sub_menu .active");
        var activeID = '#side_menu_' + $(divid + " .sub_menu .active").attr("id");
        ShowSideNav(submenulink, activeID);
    }
}

function UnPopMenu(menuid)
{
    var divid = '#' + menuid;         

    setTimeout(function(){

        var evalItem = $(divid);
        var evalFind = evalItem.parent().find(":hover");

        var isHovered = (evalFind.length == 1 && evalFind[0].id == evalItem.attr("id"));
                
        if (isHovered == false)
        {
            isHovered = "#" + evalHover == divid;
        }
                    
        if (isHovered == false && evalHover.length > 0)
        {
            isHovered = endsWith(divid, evalHover);
        }
                    
        if (isHovered == false)
        {
            $(divid).stop(true,true).hide(menueffect, menuoptions, menuduration);
                            
            $("#" + divid.substring(13)).removeClass('hoveractive');                                

            $(".sub_menu a").removeClass('hoveractive');
            $(divid + " .side_menu_container").hide();
            
            if ($(divid + " .sub_menu .active").length == 1)
            {
                var submenulink = $(divid + " .sub_menu .active");
                var activeID = '#side_menu_' + $(divid + " .sub_menu .active").attr("id");
                ShowSideNav(submenulink, activeID);
            }
        }
    },250);
}

function endsWith(str, suffix) {
    return str.indexOf(suffix, str.length - suffix.length) !== -1;
}            
