	<hr class="space"/>
	<h4 class="result-message" id="past-message"></h4>
	<div id="pastEvents" class="post-thumbnail-list col-lg-12 col-xs-12">
	</div>
	<hr class="space" />
</div>

<script>
	$(document).ready(function () {
		global.setYearAndExecuteAjax(function () {
			getEventsAjax(function(events) {
				renderEventPresentations(events);
			});
		});

		$('#eventsDropDown').change(function () {
			global.yearFilter = $('#eventsDropDown').val();
			$('#pastEvents').html("");
			$('#upcomingEvents').html("");
			global.setYearAndExecuteAjax(function () {
				getEventsAjax(function (events) {
					renderEventPresentations(events);
				});
			});
		});

		function getEventsAjax(cb) {
			var data = {
				webLinkGroupId: global.eventsPresentationsId,
				subGroupYear: global.year
			}

			$.ajax({
				type: 'POST',
				url: global.baseUrl + 'Sdpi/WebLinkManager/GetWebGroupPublic',
				data: JSON.stringify(data),
				contentType: 'application/json'
			}).done(function (eventsData) {
				if (cb) cb(eventsData);
			}).fail(function (error, err) {
				toastr.error('There was an error loading events & presentations');
			}).always(function () {
				global.loadingModal.hidePleaseWait();
			})
		}

		function renderEventPresentations(eventsData) {
			for (let group of eventsData.WebLinkGroups) {
				let date = moment(group.WebLinkGroup.dateExpires)
				let finalElement = '<div class="post-thumbnail-entry"><div class="post-thumbnail-content"><h4 class="text-bold text-small">' + group.WebLinkGroup.groupDisplayedName + '</h4><div class="post-date"><span class="post-date-day">' + date.format('DD MMMM YYYY') + '</span></div>'
				for(let webLink of group.WebLinks) {
					let link
					if (webLink.isLinkExternal === true) link = webLink.fileName
					else link = '/Content/WebLinkFiles/' + webLink.fileName
					finalElement += '<span class="col-lg-12 col-xs-12"><a href="' + link + '" target="_blank"><i class="fa fa-external-link"></i>' + " " + webLink.webLinkDisplayedName + '</a></span>'
				}
				finalElement += '</div></div>'
				if (date.isAfter(global.d)) {
					$('#upcomingEvents').append(finalElement)
				}
				if (date.isBefore(global.d)) {
					$('#pastEvents').append(finalElement)
				}
			}

			if ($('#upcomingEvents').children().length === 0) {
				$('#upcomingEvents').append('<p class="no-events-message" id="no-upcoming-events">No upcoming events to view</p>');
			} else {
				$('#no-upcoming-events').remove()
			}

			if ($('#pastEvents').children().length === 0) {
				$('#pastEvents').append('<p class="no-events-message" id="no-past-events">No past events to view</p>');
			} else {
				$('#no-past-events').remove()
			}
		}
	});

</script>

