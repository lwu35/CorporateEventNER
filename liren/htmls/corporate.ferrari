    </div>

    <script type="text/javascript">
        var events = {"xid" : [], "resource_uri" : []};
        (function ($) {

// se non esiste la libreria JSON nel browser (IE < 8), la includo
// if (typeof JSON == 'undefined') {
//    $.getScript('/sites/all/libraries/js/json2.js');
// }


// elementi usati:
// $("#iscrivi");  // button per iscrizione

            Drupal.behaviors.iscrizione = {
                attach: function (context, settings) {

                    // globali
                    events = {"xid" : [], "resource_uri" : []}
                    var language = $("html").attr("lang");
                    // uso il prefisso di language per avere dei path corretti
                    // nota: tutti i path avranno il prefisso "en" o "it" anche se sono
                    // in una lingua differente
                    var jsonpath = window.location.protocol + "//" + window.location.hostname + '/' + language + "/jsonevent/";
                    var map;

                    var nid, nids;

                    // Testo pulsante Iscriviti / Subscribe
                    if (language == 'it') {
                        $("#iscrivi").val("Iscriviti");
                    }

                    function pleaseWait() {
                        $("#pleasewait").show();
                    }
                    function endWait() {
                        $("#pleasewait").hide();
                    }

                    function toastText(language, type) {
                        var text = "";
                        if (language == "it") {
                            if (type == "reminder_error") {
                                text = "Registrazione all'evento non riuscita, riprova più tardi.";
                            }
                            if (type == "reminder_success") {
                                text = "Ti sei iscritto al reminder per l'evento.";
                            }
                        }
                        if (language == "en") {
                            if (type == "reminder_error") {
                                text = "Reminder subscription failed. Please try again later.";
                            }
                            if (type == "reminder_success") {
                                text = "Event reminder successfully subscribed.";
                            }
                        }
                        // nasconde l'icona di attesa all'ottenimento di un risultato qualsiasi
                        endWait();
                        return text;
                    }

                    // @todo: come cambiare i messaggi di errore in origine? cambiando utente?
                    // per ora sistemo così (traduco il messaggio sempre in italiano in inglese)
                    function toastServerText(language, text) {
                        // il server trasmette errori in italiano
                        if (language == "it") {
                            return text;
                        }

                        if (language == "en") {
                            if (text == "email: Inserisci un indirizzo e-mail valido.") {
                                return "email: Please insert a valid e-mail address.";
                            }
                            if (text == "email: Questo campo è obbligatorio.") {
                                return "email: This is a required field.";
                            }
                        }
                        // inserire if di eventuali altre lingue
                        // restituisco messaggio così com'è se non trovo corrispondenze
                        return text;
                    }

                    // @see: https://github.com/akquinet/jquery-toastmessage-plugin/wiki
                    function toastSuccess(message) {
                        //$().toastmessage('showSuccessToast', message);
                        alert(message);
                    }
                    function toastError(message) {
                        //$().toastmessage('showErrorToast', message);
                        alert(message);
                    }

                    function parseUrl( url ) {
                        var a = document.createElement('a');
                        a.href = url;
                        return a;
                    }
                    function getPathFromUrl( url ) {
                        var pathname = parseUrl(url).pathname;
                        // IE fix
                        if (pathname.substring(0,1) != "/") {
                            pathname = "/" + pathname;
                        }
                        return pathname;
                    }
                    // {"id_evento" : "/api/v1/website/14/", "data_reminder": "2012-08-10T16:52:43", "email": "tizio@example.com", "id": "1", "lingua": "it", "reminder_s": 3600, "resource_uri": "/api/v1/reminder/1/"}
                    function saveModelData(model, data) {
                        $.ajax({
                            url: '/uti/api/v1/'+ model +'/?format=json&api_key=7b54d7516ba152fb6ff263fcde9216c448f5176a&username=reminder',
                            type: 'POST',
                            contentType: 'application/json',
                            data: data,
                            dataType: 'json',
                            processData: false,
                            complete: function(XMLHttpRequest, textStatus) {
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown) { // per ora non testato
                                // debug
                                //allresp = XMLHttpRequest.getAllResponseHeaders();
                                toastError(toastText(language, "reminder_error"));
                                // alert("error reminder");
                                var messages = "";
                                // @see http://www.json.org/js.html

                                msg = JSON.parse(XMLHttpRequest.responseText);

                                jQuery.each(msg, function(i, val) {
                                    // messages = messages + i + ": " + val.join(". ") + "\n";

                                    // chiave: valore
                                    text = i + ": " + val;
                                    toastError(toastServerText(language, text));
