<main class="layout">
    <div class="container">
        <div id="events_and_presentations_app" v-cloak>        
      <div class="row mb-3">
        <div class="col-md-12">
          <h2 class="title">UPCOMING EVENTS & PRESENTATIONS</h2>
        </div>
      </div>
      <div class="row mb-5" v-if="hasUpcomingEvents">  
        <div class="col-md-10">
          <div  v-for="event in upcoming_events_list" v-bind:key="event.WorkflowId" class="news-item mb-4">
            <h6 class="news-header">{{ $root.displayDate(event.StartDate) }}  </h6>
            <a v-bind:href="event.UmbracoUrl">  
                <span class="news-summary mb-1 d-block">{{ event.Title }}</span>  
            </a>
            <template v-if="event.WebCastLink && $root.datewithin30Days(event.StartDate)">
                <a v-bind:href="event.WebCastLink" target="_blank" class="event-presentation" style="display:block">View the Webcast</a>
            </template>
            <template v-for="attachment in event.Attachments">
                <template v-if="attachment.Url">
                    <a v-bind:href="attachment.Url" target="_blank" class="event-presentation" style="display:block">{{ attachment.Title }}<template v-if="attachment.Extension && attachment.Size"> ({{ attachment.Extension }} {{ attachment.Size }})</template></a>
                </template>
            </template>
          </div>
        </div>
      </div>       
      <div class="row mb-5" v-if="!hasUpcomingEvents">          
        <div class="col-md-8">
          <h6>No upcoming events at this time</h6>
        </div>        
      </div>       
      <div class="row mb-3">
        <div class="col-md-12">
          <h2 class="title">PAST EVENTS</h2>
        </div>
      </div>
      <div class="row mb-5">
        <div class="col-md-10" >
          <div  v-for="event in past_events_list" v-bind:key="event.WorkflowId" class="news-item event mb-4">
            <h6 class="news-header">{{ $root.displayDate(event.StartDate) }}  </h6>
            <a v-bind:href="event.UmbracoUrl">
              <span class="news-summary mb-1 d-block">{{ event.Title }}</span>
            </a>
            <template v-if="event.WebCastLink && $root.datewithin30Days(event.StartDate)" >
                <a v-bind:href="event.WebCastLink" target="_blank" class="event-presentation" style="display:block">View the Webcast</a>
            </template>
            <template v-for="attachment in event.Attachments">
                <template v-if="attachment.Url">
                    <a v-bind:href="attachment.Url" target="_blank" class="event-presentation" style="display:block">{{ attachment.Title }}<template v-if="attachment.Extension && attachment.Size"> ({{ attachment.Extension }} {{ attachment.Size }})</template></a>
                </template>
            </template>
          </div>
        </div>
        <div class="col-md-2">
          <h6 class="mb-3 h5 font-weight-bold text-primary">FILTER BY YEAR</h6>
          <div class="row no-gutters">
            <div class="col-md-6 pr-2 pb-2" v-for="year in years_list" v-bind:key="year">
                <button class='btn btn-primary btn-block' v-on:click="displayListByYear(year)">{{ year }}</button>
            </div>
            <template v-if="years_show_archive">
                <div class="col-md-6 pr-2 pb-2" v-for="year in years_archive_list" v-bind:key="year">
                    <button class='btn btn-primary btn-block' v-on:click="displayListByYear(year)">{{ year }}</button>
                </div>
            </template>
            <template v-if="!years_show_archive">
                <div class="col-md-6 pr-2 pb-2">
                    <button class='btn btn-secondary btn-block' v-on:click="showArchive()">Archive</button>
                </div>
            </template>
          </div>
        </div>
      </div>       
    </div>
    </div>
</main>

<script>var events_and_presentations_app = new Vue({
    el: '#events_and_presentations_app',
    data: {
        years_list: [],
        year_selected: '',
        years_archive_list: [],
        years_show_archive: false,
        past_events_list: [],
        upcoming_events_list: [],
    },

    created: function () {
        this.initializePastEvents();
        this.initializeUpcomingEvents();
    },

    methods: {
        initializePastEvents: function () {
            let self = this;
            let url = "/Umbraco/Api/Q4Data/ListYears";
            axios
                .get(url)
                .then(function (response) {
                    let result = response.data.GetEventYearListResult;
                    let baseLineYear = new Date().getFullYear() - 10;
                    let years = [];
                    let archive_years = [];

                    result.forEach(function (item) {
                        if (item >= baseLineYear)
                            years.push(item);
                        else
                            archive_years.push(item);
                    });

                    self.years_list = years;
                    self.years_archive_list = archive_years;
                    self.year_selected = self.years_list[0];
                    self.displayListByYear(self.year_selected);

                })
                .catch(function (error) {
                    console.log(error);
                });
        },

        displayListByYear: function (year) {
            let self = this;            
            let url = "/Umbraco/Api/Q4Data/ListPastEvents?year=" + year;
            axios
                .get(url)
                .then(function (response) {
                    var results = response.data.GetEventListResult;
                    self.past_events_list = results;
                })
                .catch(function (error) {
                    console.log(error);
                });
        },

        initializeUpcomingEvents: function () {
            let self = this;            
            let url = "/Umbraco/Api/Q4Data/ListFutureEvents?olderFirst=true";     
            axios
                .get(url)
                .then(function (response) {                    
                    var results = response.data.GetEventListResult;                    
                    self.upcoming_events_list = results;
                })
                .catch(function (error) {
                    console.log(error);
                });
        },

        displayDate: function (date) {
            let displayDate = moment(date).format("MMMM DD, YYYY");
            return displayDate;
        },

        datewithin30Days: function (date) {
            let startDate = moment(date);
            let todaysDate = moment();
            let days = todaysDate.diff(startDate, 'days');

            return days <= 30;
        },
        showArchive: function () {
            this.years_show_archive = true;
        }
    },

    computed: {
        hasUpcomingEvents: function () {
            return (this.upcoming_events_list.length > 0);
        }
    }

});
</script>



