        <script>
        $(function() {
            // Move all modals to bottom of body
            $(".modal").detach().appendTo("body");
            
            // Activate component if URL request has hash
            var url = document.location.toString();
            if (url.match('#')) {
                var hash = url.split('#')[1];
                var target = $("#" + hash);
                
                if (target.is(".tab-pane")) {
                    $('a[href="#' + hash + '"]').tab('show');
                } else if (target.is(".modal")) {
                    target.modal('show');
                }
            }
            
            // Change hash for page-reload
            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                var target = $( $(e.target).attr("href") );
                //$('html,body').animate({ scrollTop: target.offset().top - 100 }, 0);
                //window.location.hash = e.target.hash;
            });
            
            $('a[data-toggle="modal"]').on('show.nbs.modal', function (e) {
                //window.location.hash = e.target.hash;
            });
        })
        </script>
