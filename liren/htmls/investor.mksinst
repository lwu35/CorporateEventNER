    function setupHeaderIR() {
        $(".promotional-banner").closest(".container-fluid").hide();
        $(".account_icon").hide();
        $(".compare").hide();

        var mksBaseUrl = "https://www.mksinst.com";
        $(".absMksinstHeader").find("a").each(function () {
            var curVal = $(this).attr("href");
            if (curVal && curVal.startsWith("/")) {
                $(this).attr("href", mksBaseUrl + curVal);
            }
        });
        $(".absMksinstHeader").find("img").each(function () {
            var curVal = $(this).attr("src");
            if (curVal && curVal.startsWith("/")) {
                $(this).attr("src", mksBaseUrl + curVal);
            }
        });
        $(".absMksinstHeader").find("div,a").each(function () {
            var curStyle = $(this).attr("style");
            if (curStyle && curStyle.startsWith("background")) {
                var newCurVal = curStyle.replace("/medias", mksBaseUrl + "/medias");
                $(this).attr("style", newCurVal);
            }
        });
    }
</script>
