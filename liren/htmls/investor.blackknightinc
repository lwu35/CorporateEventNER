</div></span><span class='Q4FooterDiv14'><div id="_ctrl0_ctl45_divModuleContainer" class="module module-embed hidden">
    <div class="module_container module_container--outer">
        
        <div class="module_container module_container--inner">
            <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>

var q4App = $.extend(true, q4Defaults, {
    options: {
        headerOffset: $('.pane--header').height() + 20,
        mailingListConfig: {
            submitText: 'Sign Up'
        }
    },
    windowScrolled: function(){
        $(window).on('load, scroll', function(){
            if($(window).scrollTop() > 0){
                $('.layout').addClass('js--scrolled');
            }else{
                $('.layout').removeClass('js--scrolled');
            }
        });
    },
    toggleLayoutClass: function(btn, className){
        var layout = $('.layout');
        $(btn).on('click', function(){
            layout.toggleClass(className);
            if($(btn).hasClass('layout_toggle--search')){
                $('.module-search_input[type="text"]').focus();
            }
        });
        $('.layout').on('click', function(e){
            var elementClicked = $(e.target);
            if( elementClicked.hasClass('layout') || elementClicked.hasClass('module-close_button')){
                layout.removeClass(className);
            }
        });
    },
    addEventSeparator: function(block, item){
            $(block).find('.module_link').parent().addClass('module_link-wrap');
            $(block).find(item).each(function(){
                $(this).find('.module_link-wrap:last').addClass('hide-separator');
            });
    },
    addModuleNav: function(){
      $('.nav--investors .level1 > li').addClass('ir').insertBefore($('.nav--main .level1 > li:nth-child(4)'));
      // Add Ir navigation To Corporate Navigation for SiteMap pages
      // $('.module-ir-site-map .level1 > li').insertBefore($('.nav--sitemap .level1 > li:nth-child(4)').removeClass('hidden'));
      $('<span>Investors</span><img src="//s22.q4cdn.com/274128561/files/design/nav-ir.jpg" alt="">').prependTo('.nav--main .level1 > li.ir .level2');
    },
    mainNavExpand: function($nav){
        $nav.on('click', '.level1 > li.has-children > a', function(e){
            e.preventDefault();
            $(this).parent().toggleClass('js--selected').siblings().removeClass('js--selected');
            if ($nav.find(".js--selected").length > 0) {
                $nav.addClass("js--expanded");
            } else {
                $nav.removeClass("js--expanded");
            }
        });

        $('.layout').on('click', function(e){
            elementClicked = $(e.target);
            if(elementClicked.hasClass('layout') || elementClicked.hasClass('module-close_button')){
                $('.nav--main').removeClass('js--expanded');
                $('li.js--selected').removeClass('js--selected');
            }
        });
    },
    pageAnchors: function(){
        if( window.location.href.indexOf("?section=") > -1 ){
            var param = window.location.href.split('?section=').pop();
            // window.location.hash = '';
            setTimeout(function(){
                if($('.module-' + param).length > 0){
                    q4App.scrollTo($('.module-' + param));
                }
            }, 1000);
        }
        $('.nav--main .level3 a').on('click', function(e){
            var thisTarget = $(this).attr('href');
            if(thisTarget.indexOf("?section=") > -1 && window.location.href.indexOf(thisTarget.split('?section=')[0]) > -1){
                e.preventDefault();
                var goTo = thisTarget.split('?section=').pop();
                if($('.module-' + goTo).length > 0){
                    q4App.scrollTo($('.module-' + goTo));
                }
                $('.layout').removeClass('js--menu');
                $('.nav--main').removeClass('js--expanded');
                $('li.js--selected').removeClass('js--selected');
            }
        });
    },
    headerLinks: function(){
        $('.nav--main .level1 > li:first-child > a').attr('data-quick-link-id', 'industries');
        $('.nav--main .level1 > li:nth-child(2) > a').attr('data-quick-link-id', 'technology');

        $('.module-toggle').on('click', 'a[data-open-menu-item]', function(e){
            e.preventDefault();
            
            var menu_item_data_id = $(this).attr('data-open-menu-item'),
                matching_menu_item = $('a[data-quick-link-id="' + menu_item_data_id + '"]');

            if ( matching_menu_item.length > 0 ) {
                $('.layout').addClass('js--menu');
                matching_menu_item.parent().addClass('js--selected');
                $('.nav--main').addClass('js--expanded');
            }
            
        });
    },
    breadcrumbLink: function(){
        $('.module-breadcrumb_home-link').attr('href','http://blackknightinc.com/').attr('target','_blank');
    },
    init: function() {
        var app = this;

        app.cleanUp();
        app.breadcrumbLink();
        app.unWrapLink('a.module-stock-header_stock-price');
        app.submitOnEnter('.module-unsubscribe');
        app.submitOnEnter('.module-search');
        app.validateSubmit('.module-search');
        app.superfish($('.nav--secondary .level2'), {cssArrows:false});
        app.mobileMenuToggle($('.layout'), '.pane--navigation', '.layout_toggle i');
        app.cleanQuickLinks($('.module-links'));
        app.copyright($('.copyright_year'));
        app.reveal('.pane--footer2 .module-subscribe', '.module-subscribe_email', '.module-subscribe_mailing-list, .module_actions', true);
        app.docTracking();
        app.fancySignup();
        app.resetDate(['.nav a[href*="s3.q4web.com"]:not([href$=".pdf"])']);
        app.previewToolbar();
        app.windowScrolled();
        app.toggleLayoutClass('.layout_toggle--search', 'js--search');
        app.toggleLayoutClass('.layout_toggle--menu', 'js--menu');
        //app.addModuleNav();

        //app.mainNavExpand($('.nav--main'));

        $(window).on('resize', function(){
            if ($(window).width() > 767) {
                app.mainNavExpand($('.nav--main--desktop'));
            } else {
                app.mainNavExpand($('.nav--main--mobile'));
            }
        }).resize();

        app.addEventSeparator('.module-event','.module_item');
        app.pageAnchors();
        app.headerLinks();
    }
});

q4App.init();
</script>
        </div>
    </div>
