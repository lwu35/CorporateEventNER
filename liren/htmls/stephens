        $(document).ready(function() {

            var categoryDropdown = $('#ddlCategories');

            categoryDropdown.on('change', function() {

                $('.eventList li').each(function() {


                    if (categoryDropdown.val() == $(this).data("id") || categoryDropdown.val() == "All") {
                        $(this).show();
                    } else {
                        $(this).hide();
                    }
                       
                });

            });

        });

    </script>

