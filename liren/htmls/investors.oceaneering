$(function() {
    $('.menu-search-module.one').appendTo('.module-top--navigation .menuSearch.one');
    $('.menu-search-module.two').appendTo('.module-top--navigation .menuSearch.two');
    $('.menu-search-module.three').appendTo('.module-top--navigation .menuSearch.three');
    $('.menu-search-module.four').appendTo('.module-top--navigation .menuSearch.four');
    $('.module-stock-header_description1, .module-stock-header_stock-price, .module-stock-header_change.module-stock-header_up').wrap('<a href="/stock-information/stock-quote-and-chart/default.aspx">');

    var search = function(here){
       // console.log($(here.target).val());
        var term = encodeURI($(here.target).val().replace(/ /g,'+'));
        var url = '/search-results/default.aspx?SearchTerm='+term;
        if (term.length && term !== 'Search'){
            window.location.href = url;
        }
    };
    $('.module-search').each(function() {
    	$(this).find('input[type="text"]').removeAttr("onkeypress").on("keydown", function(b) {
    		if(b.keyCode == 13) {
    			b.preventDefault();
    			search(b);
    		} else {
    			return true;
    		}
            // return 13 == b.keyCode ? (b.preventDefault(),
            //     $(this).closest(a).find('input[type="submit"]').trigger("click"),
            //     !1) : void 0
    	});
    });
});

var q4App = $.extend(q4Defaults, {
    stickyNav: function() {
        var wscroll = $(window).scrollTop();
        if (wscroll > 100) {
            $(".pane--header").addClass("pane--fixed");
            $(".pane--navigation").addClass("pane--nav-fixed");
        } else {
            $(".pane--header").removeClass("pane--fixed");
            $(".pane--navigation").removeClass("pane--nav-fixed");
        }
        var getIRPosition = $('.pane.pane--banner').outerHeight() + $('.pane.pane--banner').offset().top;
        var paddingAfterFix = $('.pane.pane--breadcrumb').outerHeight();
        if ($(window).width() > 845) {
            if (wscroll > getIRPosition) {
                $('.pane.pane--content').css({
                    'margin-top': paddingAfterFix
                });
                $('body').addClass('js--fixed-menu');
            } else {
                $('body').removeClass('js--fixed-menu');
                $('.pane.pane--content').css({
                    'margin-top': 0
                });
            }
        }
    },
    newSearchFunctionality: function(selector, trigger, pane) {
      $(trigger).on('click keypress',function(e){
        if (e.which == 13 || e.type == 'click') {
            $(pane).toggleClass('js--search-active');
            $(selector).toggleClass('js--revealed');
        }
      });
    },
    moveElement: function($element, target) {
        $element.insertAfter(target);
    },
    verticalTabs: function($tabs, tab, panels) {
        var app = this;
        app.accessibilize($tabs, $tabs.find(tab), $(panels));
        $(panels).addClass('js--hidden').first().removeClass('js--hidden');
        $(tab).first().addClass('js--active').attr('aria-expanded', 'true');

        $tabs.on('click keypress', tab, function(e) {
            e.preventDefault();
            var $this = $(this);
            if (e.which == 13 || e.type == 'click') {
                $(panels).addClass('js--hidden');
                $('[data-target="' + $this.data('id') + '"]').removeClass('js--hidden');
                $(tab).attr('aria-expanded', 'false').removeClass('js--active');
                $this.attr('aria-expanded', 'true').addClass('js--active');
            }
        });
    },
    opt: $.extend(q4Defaults.options.mailingListConfig, {
        tpl: (
            '<div id="SubscriberConfirmation" class="module module-subscribe module-subscribe--fancy">' +
              '<div class="module_container--outer">' +
                '<div class="module_container--inner">' +
                  '<p class="module_message module_message--success">Thank you for signing up for the mailing lists.</p>' +
                '</div>' +
              '</div>' +
            '</div>'
        )
    }),
    unrevealSiblings: function(target, parent) {
        $(target).on('click keypress', function(e) {
            if (e.keyCode == 13 || e.type == 'click') {
                if ($(target).is('a')) e.preventDefault();
                $(this).closest(parent).siblings(parent).find('.js--active, .js--revealed').removeClass('js--active js--revealed');
                $(this).closest(parent).siblings(parent).find('[aria-hidden]').attr('aria-hidden', 'true')
            }
        });
        $('body').click(function(event){
            if ($(event.target).closest('.module-top--navigation').length) {
                return; 
            } else {
                $(target).closest(parent).siblings(parent).find('.js--active, .js--revealed').removeClass('js--active js--revealed');
                $(target).closest(parent).siblings(parent).find('[aria-hidden]').attr('aria-hidden', 'true');
            }
        });
    },
    mobileTapSupport: function(selector) {
        if (this.isMobile.Android()) {
            $(selector).on('click', 'li.has-children > a', function(e) {
                var $this = $(this),
                    $parent = $this.parent();
                if (!$parent.hasClass('sfHover')) {
                    e.preventDefault();
                    $parent.siblings().removeClass('sfHover');
                    $parent.addClass('sfHover');
                }
            });
        }
    },
    pageTabs: function($tabs) {
        var tab = window.location.hash.split('#')[1];
        tab != undefined ? ($('.module-tab_' + tab).removeClass('js--hidden'), $('.module-nav-tabs a[href="#' + tab + '"]').closest('li').addClass('js--active')) : ($('.module-tab').first().removeClass('js--hidden'), $('.module-nav-tabs li:first-child').addClass('js--active'));
        $('nav').on('click', 'a[href*="#"]', function(e) {
            tab = $(this).attr('href').split('#')[1];
            if ($(this).closest('.level2').length && $(this).parent().closest('li.has-children').hasClass('selected')) {
                e.preventDefault();
                $tabs.addClass('js--hidden');
                $('.module-tab_' + tab).removeClass('js--hidden');
                window.location.hash = tab;
                if ($('.module-nav-tabs').length) {
                    $('.module-nav-tabs a[href="#' + tab + '"]').closest('li').addClass('js--active');
                }
            } else {
                e.preventDefault();
                $tabs.addClass('js--hidden');
                $('.module-tab_' + tab).removeClass('js--hidden');
                window.location.hash = tab;
                // only for in page HTML nav
                $(this).closest('li').addClass('js--active').siblings().removeClass('js--active');
            }
        });
    },
    corporateNews: function(selector) {
        $(selector).mashfeed({
            dateFormat: 'DD MMMM YYYY',
            titleLength: 0,
            limit: 3,
            feeds: [{
                name: 'rss',
                type: 'socialstream',
                client: 'oceaneering',
                channel: 'rss',
                limit: 3,
                parseItem: function(item) {
                    return {
                        image: $(item.description).find('img:first').attr('src'),
                        link: item.link,
                        date: moment(item.date, 'YYYY-MM-DD hh:mm:ss Z'),
                        title: item.title,
                        description: item.description
                    };
                },
                template: (
                    '<div class="oii-mm-sidebar-post">' +
                    '<span class="oii-mm-sidebar-postthumb"><img src="{{image}}" class="sideImage"></span>' +
                    '<span class="oii-mm-sidebar-posttitle"><a href="{{link}}">{{{title}}}</a></span><br>' +
                    '<span class="oii-mm-sidebar-postexcerpt">{{{description}}}</span>' +
                    '</div>'
                )
            }],
            feedsComplete: function(event) {

                $(event.target).find('.oii-mm-sidebar-postthumb img').not('[src^="http"],[src^="https"]').each(function() {
                    var imgUrl = 'http://www.oceaneering.com';
                    $(this).attr('src', imgUrl + $(this).attr('src'));
                });
                $(event.target).find('.oii-mm-sidebar-post').each(function() {
                    var $content = $(this).find('.oii-mm-sidebar-postexcerpt');
                    $content.html($content.text().slice(0, 60) + ' [&hellip;]');
                });
                // broken Image fix
                $('.module-news-feed .oii-mm-sidebar-post:contains("May 11, 2018")').find('img').attr("src","https://www.oceaneering.com/wp-content/uploads/2018/05/Carbon-Trust-Competition-rev12.jpg");
               $('.module-news-feed .oii-mm-sidebar-post:contains("September 28, 2018")').find('img').attr("src","https://www.oceaneering.com/wp-content/uploads/2018/10/TRND-IONIX.jpg");
            }
        });
    },
    init: function() {
        var app = this;

        app.cleanUp();
        app.unWrapLink('a.module-stock-header_stock-price');
        app.submitOnEnter('.module-unsubscribe');
        app.submitOnEnter('.module-search');
        app.validateSubmit('.module-search');
        app.mobileMenuToggle($('.layout_header'), '.pane--navigation', '.module-top--toggle');
        app.mobileMenuToggle($('.layout_content'), '.pane--breadcrumb', '.layout_toggle');
        app.reveal('.module-mobile--nav .hasChildren', '> a', '.yamm-content', false);
        app.reveal('.module-top--navigation .hasChildren', '> a', '.yamm-content', false);
        app.unrevealSiblings('.module-top--navigation .hasChildren > a', 'li');
        app.cleanQuickLinks($('.module-links'));
        app.copyright($('.copyright_year'))
        app.reveal('.pane--footer .module-subscribe', '.module-subscribe_email', '.module-subscribe_mailing-list, .module_actions', true);
        app.docTracking();
        app.fancySignup();
        app.resetDate(['.nav a[href*="s4.q4web.com"]:not([href$=".pdf"])']);
        app.previewToolbar();
        app.reveal('.layout_header', '.module-search--trigger', '.module-search', false);
        app.moveElement($('.module-subscribe--footer .module_actions'), '.module-subscribe--footer .module-subscribe_form');
        app.mobileTapSupport('.nav--main .level2');
        app.corporateNews('.module-news-feed');
    }
});
q4App.init();
$(window).on('scroll', function() {
    q4App.stickyNav();
}).trigger('scroll');
</script>
