            var marker = $('.megamenuMarker');
            var parent = e.target;
            var markerOffset = parent.offsetLeft + (parent.offsetWidth / 2) - (marker.width() / 2);
            $('#mm-search input').focus();
            if (!marker.is(':visible')) {
                marker.css('left', markerOffset);
                marker.css('top', setMarkerTop());
