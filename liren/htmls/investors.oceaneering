                    $('.megamenuMarker').removeClass("shrink-megamenuMarker");
                    $('.oii-mega-menu').removeClass("fade-oii-mega-menu");
                    var ddOffset = $(this)["0"].offsetTop;
                    if (subMenu.outerHeight()) { ddOffset = subMenu.outerHeight() - ($(this)["0"].offsetTop - $('.submenu-panel')["0"].offsetTop); }
