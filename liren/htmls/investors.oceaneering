                    marker.css('top', setMarkerTop());
                }
            }
            var marker = $('.megamenuMarker');
            marker.css('top', setMarkerTop());
            var bttoffset = 250;
            var bttduration = 300;
            if ($(this).scrollTop() > bttoffset) { $('.back-to-top').fadeIn(bttduration); } else { $('.back-to-top').fadeOut(bttduration); }
        });
        $('.back-to-top').click(function(event) {
            var bttduration = 300;
            event.preventDefault();
            $('html, body').animate({ scrollTop: 0 }, bttduration);
            return false;
        })




