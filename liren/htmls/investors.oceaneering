            function hideMarker() {
                if (!$('#menu-header > .dropdown').hasClass('open')) {
                    var marker = $('.megamenuMarker');
                    marker.hide();
                }
            }
        });



        $(window).scroll(function() {
            var subMenu = $('.submenu-panel');
            var wscroll = $('.oii-show-search').hasClass('open') ? 0 : $(this).scrollTop();
            var nb_height = $(".navbar").height()
            var sub_offset = subMenu.length ? $('.submenu-panel').offset().top : 0;
            var nb_fade_trigger = nb_height + wscroll;
            var attenuator = nb_height + (sub_offset - nb_fade_trigger);
            var scale = (attenuator / nb_height) * 100;
            var op_factor = 1;
            if (subMenu.length) {
                if (attenuator <= nb_height && attenuator >= 0) { op_factor = Math.round(scale) / 100; } else if (attenuator <= 0) { op_factor = 0; }
                if (subMenu.hasClass("affix")) { op_factor = 0; }
                $(".navbar").css("opacity", op_factor);
            }
            if (wscroll > 100) {
                $(".navbar").addClass("shrink-nav");
                $(".logo").addClass("shrink-logo");
                if ($('#menu-header > .dropdown').hasClass('open')) {
                    $('.megamenuMarker').addClass("shrink-megamenuMarker");
                    $('.oii-mega-menu').addClass("fade-oii-mega-menu");
                }
            } else {
