<script>
    (function () {

        // Variables.
        var i, timeouts = [], timeoutToken,
            durations = [0, 10, 50, 100, 200, 300, 400, 500, 600, 700, 800, 900, 1000, 2000, 3000, 5000, 8000];

        // Set top offsets immediately, then repeatedly at the designated delay durations.
        setTopOffsets(timeouts);
        for (i = 0; i < durations.length; i++) {
            timeoutToken = setTimeout(function () {
                setTopOffsets(timeouts);
            }, durations[i]);
            timeouts.push(timeoutToken);
        }

        // Sets the top offsets for the page and global banner.
        function setTopOffsets(timeouts) {

            // Variables.
            var i, topEl = document.querySelector('#top.static'),
                cookieEl = document.querySelector('.ww-cookie-disclaimer.static:not(.bottom)'),
                globalEl = document.querySelector('.global-banner.static'),
                cookieHeight,
                globalHeight;

            // Does the static top element exist (it will be removed after the Angular has run)?
            if (topEl) {

                // Set the top margin. It's also stored in a global variable so the Angular code can access it.
                cookieHeight = cookieEl ? cookieEl.offsetHeight : 0;
                globalHeight = globalEl ? globalEl.offsetHeight : 0;
                window.staticTopOffset = cookieHeight + globalHeight;
                topEl.style.marginTop = window.staticTopOffset + 'px';

                // Also, set the top of the global banner (and store a global variable for Angular).
                if (globalEl && cookieEl) {
                    globalEl.style.top = cookieHeight + 'px';
                    window.staticGlobalTopOffset = cookieHeight;
                } else {
                    window.staticGlobalTopOffset = 0;
                }

            } else {

                // Stop the remaining timeouts.
                for (i = 0; i < timeouts.length; i++) {
                    clearTimeout(timeouts[i]);
                }

            }

        }

    })();
</script>

    

