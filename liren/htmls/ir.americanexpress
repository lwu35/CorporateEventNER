                    '<div class="module_news">'+
                        '<a href="{{url}}" {{#blank}} target="_blank"{{/blank}} class="module_link module_news-link">'+
                            '<span class="q4icons_icon" aria-hidden="true"></span>'+
                            '<span class="module_link-text">Press Release</span>'+
                            '{{#blank}}<span class="sr-only">(opens in new window)</span>{{/blank}}'+
                        '</a>'+
                    '</div>' +
