    itemsComplete: function(e) {
        $(e.target).find('.module_pager').pager({
            content: $(e.target).find('.module_item'),
            perPage: 10,
            template: (
                '<span class="module_pager-item">{{{page}}}</span>'
            ),
            append: false,
            trigger: 'span',
            showFirstLast: false,
            showPrevNext: true,
            labels: {
                prev: '<span class="module_pager-icon">&#xedc6;</span>Previous',
                next: 'Next<span class="module_pager-icon">&#xedbe;</span>',
            },
            afterChange: function(e, data) {
                // debugger;
                $(e.target).find('.js--dots-elipse').remove();
                var $pages = $(e.target).find(".pager-page"),
                    $pageActive = $(e.target).find(".pager-active"),
                    pagesNo = $pages.length;
                $pages.addClass('js--hidden');
                $pages.first().removeClass('js--hidden');
                $pages.last().removeClass('js--hidden');
                $pageActive.removeClass('js--hidden');
                var pg = 12;
                $('.pager-active ~ .pager-page').splice(0, 11).map(function(item) {
                    $(item).removeClass('js--hidden');
                    pg -= 1;
                });

                var prevLength = $('.pager-page').not('.pager-active,.pager-active ~ .pager-page').length;
                $('.pager-page').not('.pager-active,.pager-active ~ .pager-page').splice(prevLength - pg, prevLength - 1).map(function(item) {
                    $(item).removeClass('js--hidden');
                });
                $('<span class="js--dots-elipse">...</span>').insertBefore($(e.target).find(".pager-page + .js--hidden").not(".js--hidden + .js--hidden"));
            }
        });
        if ($(e.target).find('.module_pager > .pager-page').length <= 1) {
            $(e.target).find('.module_pager').addClass('js--hidden');
        } else {
            $(e.target).find('.module_pager').removeClass('js--hidden');
        }
    },
    complete: function(e) {
        $(".module-event-archive").on("click", ".module_webcast-link, .module_financial-link--webcast", function(e) {
            var webcastLink = $(this).attr('href');
            e.preventDefault();
            $.fancybox.open({
                src: $('.module-disclaimer-webcast'),
                type: 'inline',
                opts: {
                    afterLoad: function() {
                        $('.module-disclaimer-webcast.fancybox-content .module-disclaimer-webcast_link').attr('href', webcastLink);
                        $('.module-disclaimer-webcast_link').on('click', function(e) {
                            $.fancybox.close();
                        });

                    }
                }
            });
        });
    }
});
