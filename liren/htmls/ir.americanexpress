            }
            $.each(item.financialReports, function(j, report) {
                $.each(report.docs, function(k, doc) {
                    if (doc.docCategory == 'webcast' && doc.docUrl == item.webcast) {
                        doc.duplicateWebcast = true;
                    }
                    var url = doc.docUrl.toLowerCase();
                    if (url.indexOf('aspx') < 0) {
                        doc.blank = true;
                    }
                });
            });
            $.each(item.pressReleases, function(k, press) {
                var url = press.url.toLowerCase();
                if (url.indexOf('aspx') < 0) {
                    press.blank = true;
                }
            });
            $.each(item.docs, function(l, doc) {
                var url = doc.url.toLowerCase();
                if (url.indexOf('aspx') < 0) {
                    doc.blank = true;
                }
