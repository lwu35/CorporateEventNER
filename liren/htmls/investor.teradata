        });

        $('.layout_toggle .burger-menu, .nav--secondary .button_close').on('click keypress', function() {
            $('.layout').toggleClass('js--mobile');
            $('.nav--secondary li.has-children').removeClass('js--expanded');
        })
    },

    // toggle contrast function
    contrast: {
        toggle: '.module-contrast_button',
        getCookie: function() {
            return document.cookie.replace(/(?:(?:^|.*;\s*)contrast\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        },
        setCookie: function(boolean) {
            document.cookie = 'contrast=' + boolean + '; path=/';
        },
        init: function() {
            var inst = this;
            if (!inst.getCookie().length) {
                inst.setCookie('false');
            } else if (inst.getCookie() == 'true') {
                $(inst.toggle).addClass('js--active');
                $('body').addClass('js--contrast');
            }

            $(inst.toggle).on('click', function(e) {
                e.preventDefault();
                if (inst.getCookie() == 'false') {
                    inst.setCookie('true');
                } else {
                    inst.setCookie('false');
                }
                $(this).toggleClass('js--active');
                $('body').toggleClass('js--contrast');
            });
        }
    },
    sections: function() {
        $(window).on('load', function() {
            if (window.location.hash.length) {
                var hash = window.location.hash.substring(1);
                setTimeout(function() {
                    if ($('.module-' + hash).length) {
                        q4App.scrollTo($('.module-' + hash), 500);
                        window.location.hash = hash;
                    }
                }, 700);
            }
        });

        $('body').on('click', 'a[href*="#"]', function(e) {
            var hash = $(this).attr('href').split('#').pop();
            if ($('.module-' + hash).length) {
                e.preventDefault();
                $('.layout.js--mobile').removeClass('js--mobile');
                q4App.scrollTo($('.module-' + hash), 500);
                window.location.hash = hash;
            }
        });

    },

